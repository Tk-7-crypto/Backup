        <description>CSM-328 ALL - Product and Asset validation rule for CRM and CSM</description>
        <errorConditionFormula>AND( 
NOT($Profile.Name = &apos;System Administrator&apos;),
NOT($Permission.CSM_Bypass_CSM_Validation_Rules),
NOT(AND(NOT(ISBLANK(Product2Id)),
UPPER(Product2.Name) = UPPER(Name)
)))</errorConditionFormula>
        <errorDisplayField>Product2Id</errorDisplayField>
        <errorMessage>Please check Product and Asset Name should be same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AST_CSM_VR02_ProductSpecifictoCSM</fullName>
        <active>true</active>
        <description>CSM-328 ALL - Product and Asset validation rules</description>
        <errorConditionFormula>AND( 
NOT($Profile.Name = &apos;System Administrator&apos;),
NOT($Permission.CSM_Bypass_CSM_Validation_Rules),
$Permission.AssetManager ,
NOT(AND(NOT(ISBLANK(Product2Id)),Product2.SpecificToCSM__c = true)))</errorConditionFormula>
        <errorMessage>Please check Specific to CSM? field is always checked in Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AST_CSM_VR03_CSHSubtype123</fullName>
        <active>true</active>
        <description>To prevent checking the asset CSHSubtype123 checkbox if Product CSHSubtype123 is checked.</description>
        <errorConditionFormula>AND(ISCHANGED(CSH_Enable_SubType123__c), CSH_Enable_SubType123__c,
NOT($Permission.CSM_Bypass_CSM_Validation_Rules), Product2.CSH_Enable_SubType123__c)</errorConditionFormula>
        <errorDisplayField>CSH_Enable_SubType123__c</errorDisplayField>
        <errorMessage>CSH Subtype 1/2/3 is already enabled at Product level. Please disable the Product before enabling Asset.</errorMessage>
    </validationRules>
