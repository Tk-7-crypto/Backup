    <validationRules>
        <fullName>Prevent_record_to_be_changed_to_Close</fullName>
        <active>true</active>
        <errorConditionFormula>AND
    (
				TEXT(Status__c) == &quot;Closed&quot;,
				SOW__c == false
    )</errorConditionFormula>
        <errorMessage>Record can not be changed to Closed if SOW checkbox is false</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subscription_End_Date_Greater_then_Start</fullName>
        <active>true</active>
        <description>Subscription End Date should be greater than Subscription Start Date</description>
        <errorConditionFormula>Subscription_End_Date__c &lt;  Subscription_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Subscription_End_Date__c</errorDisplayField>
        <errorMessage>Subscription End Date should always be greater than Subscription Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subscription_End_Date_Past_Date</fullName>
        <active>true</active>
        <description>Subscription End Date can not be a past date</description>
        <errorConditionFormula>ISCHANGED(Subscription_End_Date__c ) &amp;&amp; Subscription_End_Date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Subscription_End_Date__c</errorDisplayField>
        <errorMessage>Subscription End Date can not be a past date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Training_Target_date_not_less_than_today</fullName>
        <active>true</active>
        <description>Training Target date should not be a past date</description>
        <errorConditionFormula>Training_Target_date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Training_Target_date__c</errorDisplayField>
        <errorMessage>Training target date can&apos;t be a past date.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
