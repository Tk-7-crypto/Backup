        <referenceTo>Contact</referenceTo>
        <relationshipLabel>TS LMS Training Management</relationshipLabel>
        <relationshipName>Technologies_Product_Training_Management</relationshipName>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
