    <validationRules>
        <fullName>Actual_Date_Required_Missed_Notification</fullName>
        <active>true</active>
        <description>PSA-CLD-IQVIAPSA-1217- PSA Events must have &quot;Actual Date Field&quot; populated in order to have &quot;Send Missed Due Date Notifications&quot; set to true</description>
        <errorConditionFormula>AND(
 Actual_Date_Field__c = Null,
 Send_Missed_Due_Date_Notification__c = True,
 Object__c != &apos;PSSF__c&apos;
)</errorConditionFormula>
        <errorDisplayField>Actual_Date_Field__c</errorDisplayField>
        <errorMessage>You must populate an &quot;Actual Date Field&quot; in order to enable &quot;Send Missed Due Date Notification&quot;</errorMessage>
    </validationRules>
