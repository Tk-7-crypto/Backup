    <validationRules>
        <fullName>PTG_VR01_PSA_Program_Match</fullName>
        <active>true</active>
        <description>Validates that Target and Project are on the same Program</description>
        <errorConditionFormula>OR(
  AND(
    NOT(ISBLANK(Project__c)),
    Target__r.Program__c !=  Project__r.Program__c
  ),
  AND(
    NOT(ISBLANK(Service_Line_Project__c)),
    Target__r.Program__c != 
    Service_Line_Project__r.pse__Parent_Project__r.Program__c
  )
)</errorConditionFormula>
        <errorMessage>Please select a Project from the same Program.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
