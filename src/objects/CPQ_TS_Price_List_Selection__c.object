<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Custom settings to populate price list on Proposal record based on Account Country and Proposal record type</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Account_Country__c</fullName>
        <description>Should contain short form of Account country like JP</description>
        <externalId>false</externalId>
        <inlineHelpText>Should contain short form of Account country like JP</inlineHelpText>
        <label>Account Country</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Price_List_Name__c</fullName>
        <description>Should be the name of CPQ Price List</description>
        <externalId>false</externalId>
        <inlineHelpText>Should be the name of CPQ Price List</inlineHelpText>
        <label>Price List Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Proposal_Record_Type_Name__c</fullName>
        <description>Should be the Developer name of Proposal record type</description>
        <externalId>false</externalId>
        <inlineHelpText>Should be the Developer name of Proposal record type</inlineHelpText>
        <label>Proposal Record Type Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>CPQ TS Price List Selection</label>
    <visibility>Public</visibility>
</CustomObject>