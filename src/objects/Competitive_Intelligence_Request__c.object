    <validationRules>
        <fullName>COM_VR01_restrictedTheRequestViaOpp</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-5125</description>
        <errorConditionFormula>AND(!$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c, ISNEW(), 
 NOT(AND( IF(OR( ISPICKVAL(Opportunity__r.Owner.Sales_Rep_Location__c, &apos;Japan&apos;),ISPICKVAL(Opportunity__r.Owner.Sales_Rep_Location__c, &apos;Asia&apos;)  ),Opportunity__r.Amount_In_USD__c  &gt;  2000000 , Opportunity__r.Amount_In_USD__c &gt; 5000000 ),  ISPICKVAL(Opportunity__r.Line_of_Business__c, &apos;Clinical&apos;), 
OR(  ISPICKVAL(Opportunity__r.StageName, &apos;3. Developing Proposal&apos;),ISPICKVAL(Opportunity__r.StageName, &apos;4. Delivering Proposal&apos;)  )   )))</errorConditionFormula>
        <errorMessage>CI requests for Clinical at Stages 3 and 4 where opportunity value &gt;$2M for JAPAC and &gt;$5M for Global. For exceptions, please e-mail RDSCompetitiveIntelligence@iqvia.com</errorMessage>
    </validationRules>
