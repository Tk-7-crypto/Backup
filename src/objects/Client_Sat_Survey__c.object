    <validationRules>
        <fullName>Check_Survey_Type</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Opportunity_Amount__c &gt;= 400000,ISPICKVAL(Survey_Method__c, &apos;Web Survey&apos;) )</errorConditionFormula>
        <errorDisplayField>Survey_Method__c</errorDisplayField>
        <errorMessage>You must select Survey Type ‘Interview’ when the opportunity is over USD 400,000 in value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Lock_Pending_Approval</fullName>
        <active>false</active>
        <description>ESPCRMINT-225. Updated 418.</description>
        <errorConditionFormula>AND(
	NOT(ISNEW()),
    OR(
    ISPICKVAL(PRIORVALUE(Survey_Initiation_Status__c), &apos;Survey Declined, Pending Mgr Approval&apos;),
    ISPICKVAL(PRIORVALUE(Survey_Initiation_Status__c), &apos;Survey Delayed, Pending Approval&apos;)
    ),
	NOT(CONTAINS($Profile.Name, &quot;System Administrator&quot;)),
	$User.Email &lt;&gt; $Setup.CS_Config__c.CS_Admin_Email__c
)</errorConditionFormula>
        <errorMessage>This record is pending for approval and cannot be edited</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Edit_Access</fullName>
        <active>false</active>
        <errorConditionFormula>AND( NOT(CONTAINS($Profile.Name, &quot;System Administrator&quot;)),
!$Setup.CS_Config__c.Is_Admin_User__c, 
AND( 
