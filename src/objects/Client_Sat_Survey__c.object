        <errorConditionFormula>AND( ISPICKVAL(Survey_Initiation_Status__c, &quot;New&quot;) , OR( ISNULL( Survey_Send_Date__c ) , ISBLANK(Survey_Send_Date__c ) ) )</errorConditionFormula>
