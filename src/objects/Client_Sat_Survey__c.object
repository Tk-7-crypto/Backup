    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Check_Denial_with_opp_stage</fullName>
        <active>true</active>
        <description>Send Survey? can not be marked &apos;No&apos; if Opportunity Stage is not &apos;6. Received ATP/LOI&apos;, &apos;7a. Closed Won&apos;, &apos;7b. Closed Lost&apos; or &apos;In-Hand&apos;</description>
        <errorConditionFormula>AND( OR( ISPICKVAL(Opportunity__r.StageName, &apos;1. Identifying Opportunity&apos;), ISPICKVAL(Opportunity__r.StageName, &apos;2. Qualifying Opportunity&apos;), ISPICKVAL(Opportunity__r.StageName, &apos;3. Developing Proposal&apos;), ISPICKVAL(Opportunity__r.StageName, &apos;4. Delivering Proposal&apos;) ), ISPICKVAL(Send_Survey__c, &apos;No&apos;)  )</errorConditionFormula>
        <errorDisplayField>Send_Survey__c</errorDisplayField>
        <errorMessage>Send Survey? can not be marked &apos;No&apos; if Opportunity Stage is not &apos;6. Received ATP/LOI&apos;, &apos;7a. Closed Won&apos;, &apos;7b. Closed Lost&apos; or &apos;In-Hand&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Lock_Survey</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISNEW()), ISCHANGED(Send_Survey__c), ISPICKVAL(PRIORVALUE (Send_Survey__c), &apos;Yes&apos;), NOT( OR( CONTAINS($Profile.Name, &apos;System Administrator&apos;), $Setup.CS_Config__c.CS_Admin_Email__c = $User.Email ) ) )</errorConditionFormula>
        <errorMessage>This survey has been locked and can not be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Survey_Decline_Reason</fullName>
        <active>true</active>
        <description>Required to enter a Decline Survey Reason when new survey is set to Send Survey = No.</description>
        <errorConditionFormula>AND( ISPICKVAL( Survey_Initiation_Status__c , &quot;New&quot;), ISPICKVAL( Send_Survey__c , &quot;No&quot;), ISPICKVAL( Decline_Survey_Reason__c, &quot;&quot; ) )</errorConditionFormula>
        <errorDisplayField>Decline_Survey_Reason__c</errorDisplayField>
        <errorMessage>Please enter Decline Survey Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Survey_Decline_Reason_Other</fullName>
        <active>true</active>
        <description>If Decline Survey Reason is set to &apos;Other&apos; the &quot;Decline Survey Reason Other&quot; field must be filled out.</description>
        <errorConditionFormula>AND( ISPICKVAL( Decline_Survey_Reason__c , &quot;Other (please specify)&quot;), OR( ISNULL( Decline_Survey_Reason_Other__c ) , ISBLANK(Decline_Survey_Reason_Other__c ) ) )</errorConditionFormula>
        <errorDisplayField>Decline_Survey_Reason_Other__c</errorDisplayField>
        <errorMessage>Please specify Decline Survey Reason Other.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Survey_In_Past</fullName>
        <active>true</active>
        <description>To check Survey Date is not in the past. /ESPSFDCQI-16549 </description>
        <errorConditionFormula>AND(
    Survey_Send_Date__c &lt; TODAY(),
    $Setup.CS_Config__c.Is_Admin_User__c == False
)</errorConditionFormula>
        <errorDisplayField>Survey_Send_Date__c</errorDisplayField>
        <errorMessage>Survey Date shouldn&apos;t be in past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Survey_Recipient1</fullName>
        <active>true</active>
        <description>Survey Recipient 1 must be populated.</description>
        <errorConditionFormula>AND(NOT(ISBLANK( TEXT(Send_Survey__c))),ISBLANK(Survey_Recipient_1__c))</errorConditionFormula>
        <errorDisplayField>Survey_Recipient_1__c</errorDisplayField>
        <errorMessage>Please specify Survey Recipient 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Survey_Recipient_1_Data</fullName>
        <active>true</active>
        <description>ESPCRMINT-117</description>
        <errorConditionFormula>AND(
	NOT(ISBLANK(Survey_Recipient_1_Email__c)),
	OR(
		ISBLANK(Survey_Recipient_1_First_Name__c),
		ISBLANK(Survey_Recipient_1_Last_Name__c) ,
		ISBLANK(Survey_Recipient_1_Job_Title__c)   
	)
)</errorConditionFormula>
        <errorMessage>You do not have the information required to create a contact for the Survey Recipient 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Survey_Recipient_2_Data</fullName>
        <active>true</active>
        <description>ESPCRMINT-117</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Survey_Recipient_2_Email__c)),
OR(
ISBLANK(Survey_Recipient_2_First_Name__c),
