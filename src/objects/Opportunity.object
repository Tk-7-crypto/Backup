        <fullName>Probability_Points__c</fullName>
        <description>ESPCRMINT-125</description>
        <externalId>false</externalId>
        <formula>CASE (1 ,
IF(Probability = 0, 1, 0), 0,
IF(AND( Probability &gt;0,Probability &lt;=0.1) , 1, 0), 1,
IF(AND(Probability &gt;0.1,Probability &lt;=0.2) , 1, 0), 2,
IF(AND(Probability &gt;0.2,Probability &lt;=0.3) , 1, 0), 3,
IF(AND(Probability &gt;0.3,Probability &lt;=0.4) , 1, 0), 4,
IF(AND(Probability &gt;0.4,Probability &lt;=0.5) , 1, 0), 5,
IF(AND(Probability &gt;0.5,Probability &lt;=0.6) , 1, 0), 6,
IF(AND(Probability &gt;0.6,Probability &lt;=0.7) , 1, 0), 7,
IF(AND(Probability &gt;0.7,Probability &lt;=0.8) , 1, 0), 8,
IF(AND(Probability &gt;0.8,Probability &lt;=0.9) , 1, 0), 9,
IF(AND(Probability &gt;0.9,Probability &lt;=1) , 1, 0), 10,
IF(Probability &gt;1 , 1, 0), 10,
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Probability Points</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
