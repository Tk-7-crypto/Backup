$Profile.Name == &apos;Tech Solutions User&apos;,
ISCHANGED(StageName),
ISPICKVAL(StageName, &apos;7a. Closed Won&apos;),
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE 
)</errorConditionFormula>
        <errorDisplayField>Contract_Dates_Confirmed__c</errorDisplayField>
        <errorMessage>Please review and confirm Contract Start/End Dates prior to moving to Stage 7a. Closed Won</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Survey_Recipient_Cannot_Be_Blank</fullName>
        <active>true</active>
        <description>ESPSFDCQI-3564 \ ESPSFDCQI-15203</description>
        <errorConditionFormula>AND(
    $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
    ISBLANK( Survey_Recipient__c ),
    OR( 
       Is_Survey_Eligible_for_Non_RDS__c ,
       Is_Survey_Eligible_for_RDS_Auto__c 
    ),
    OR(ISNEW(), ISCHANGED(StageName)),
    OR(
        ISPICKVAL( StageName, &quot;5. Finalizing Deal&quot; ),
        ISPICKVAL( StageName, &quot;6. Received ATP/LOI&quot; ),
        ISPICKVAL( StageName, &quot;7a. Closed Won&quot; ),
        ISPICKVAL( StageName, &quot;In-Hand&quot; )
    ),
    $Setup.User_Permissions__c.Is_Finance_User__c == FALSE
)</errorConditionFormula>
        <errorDisplayField>Survey_Recipient__c</errorDisplayField>
        <errorMessage>Survey Recipient cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Discount_Rates_Must_Be_Between_0_to_100</fullName>
        <active>false</active>
        <description>The Rate Card Percentage Discount must be between 0 and 100.</description>
        <errorConditionFormula>pse__Rate_Card_Percentage_Discount__c &lt; 0 || pse__Rate_Card_Percentage_Discount__c &gt; 100</errorConditionFormula>
        <errorMessage>The Rate Card Percentage Discount must be between 0 and 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Opp_Rate_Card_Set_Currency_Mismatch</fullName>
        <active>true</active>
        <description>The currency must be the same for the opportunity and the rate card set.</description>
        <errorConditionFormula>(pse__Rate_Card_Set__c &lt;&gt; NULL) &amp;&amp;(ISPICKVAL(CurrencyIsoCode, &apos;&apos;) ||
		ISPICKVAL(pse__Rate_Card_Set__r.CurrencyIsoCode , &apos;&apos;) ||
		(TEXT(pse__Rate_Card_Set__r.CurrencyIsoCode) &lt;&gt; TEXT(CurrencyIsoCode)))</errorConditionFormula>
        <errorMessage>You cannot associate the rate card set with the opportunity. They must both use the same currency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_VR292_LossTypeOnlyForSpecificLob</fullName>
        <active>true</active>
        <description>ESPSFDCQI-18164</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Loss_Type__c  , &quot;Loss details to be confirmed&quot;),
    NOT(
        OR(
            ISPICKVAL( Line_of_Business__c , &quot;Clinical&quot;),
            ISPICKVAL( Line_of_Business__c , &quot;Clinical Technology&quot;),
            ISPICKVAL( Line_of_Business__c , &quot;Connected Devices&quot;),
            ISPICKVAL( Line_of_Business__c , &quot;Data Sciences&quot;),
            ISPICKVAL( Line_of_Business__c , &quot;Early Clinical Development&quot;),
            ISPICKVAL( Line_of_Business__c , &quot;Patient &amp; DCT Solutions&quot;),
            ISPICKVAL( Line_of_Business__c , &quot;Regulatory &amp; Drug Development Solutions&quot;),
            ISPICKVAL( Line_of_Business__c , &quot;Safety &amp; Med Info&quot;)
        )
    ),
    $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == False
)</errorConditionFormula>
        <errorDisplayField>Loss_Type__c</errorDisplayField>
    <errorMessage>Please review Loss Type. Loss details to be confirmed - can only be selected when line of business = Clinical,Clinical Technology,Connected Devices,Data Sciences,Early Clinical Development,Patient &amp; DCT Solutions,RADDS,Safety &amp; Med Info.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_VR291_SpecifyDMPlatformusedRequired</fullName>
        <active>true</active>
        <description>ESPSFDCQI-18113</description>
        <errorConditionFormula>AND(
 ISPICKVAL(DM_Platform__c, &quot;Other&quot;) ,
 ISBLANK( Specify_DM_Platform_used__c ) ,
 $Permission.Sales_with_Bid_History ,
 $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == False
)</errorConditionFormula>
        <errorDisplayField>Specify_DM_Platform_used__c</errorDisplayField>
        <errorMessage>If DM Platform = &apos;Other&apos; then you must document any new platform used that is not listed here.</errorMessage>
    </validationRules>
