        <description>ESPSFDCQI-16396/ESPSFDCQI-17145</description>
        <externalId>false</externalId>
        <formula>IF(AND(
ISPICKVAL( Line_of_Business__c , &apos;Avacare&apos;),
OR(ISPICKVAL( InterventionType__c, &apos;Device&apos;),
ISPICKVAL( InterventionType__c, &apos;Device - Consulting&apos;),
ISPICKVAL( InterventionType__c, &apos;Device - Pivotal&apos;),
ISPICKVAL( InterventionType__c, &apos;Device - Post&apos;)
)),
1000, 
IF(AND(
ISPICKVAL( Line_of_Business__c , &apos;Avacare&apos;),
ISPICKVAL( InterventionType__c, &apos;Generic&apos;),
ISPICKVAL( Phase_P__c , &apos;Phase 4&apos;)
),500,
IF(
ISPICKVAL( Line_of_Business__c , &apos;Avacare&apos;),
CASE( TherapyArea__c ,
&quot;Acute Care&quot;,8000,
&quot;Allergy&quot;,12000,
&quot;Cardiovascular&quot;,25000,
&quot;Dermatology&quot;,21000,
&quot;Endocrinology&quot;,16000,
&quot;Gastrointestinal&quot;,14000,
&quot;Hematology&quot;,5000,
&quot;Hepatology&quot;,20000,
&quot;Immunology&quot;,10000,
&quot;Infectious Disease&quot;,10000,
&quot;Medical Genetics&quot;,5000,
&quot;Nephrology&quot;,16000,
&quot;Neurology&quot;,25000,
&quot;Oncology&quot;,20000,
&quot;Ophthalmology&quot;,12000,
&quot;Orthopedics&quot;,13000,
&quot;Other&quot;,5000,
&quot;Psychiatry&quot;,16000,
&quot;Respiratory&quot;,20000,
&quot;Rheumatology&quot;,15000,
&quot;Transplantation&quot;,0,
&quot;Women&apos;s Health / Sexual Health&quot;,15000
,null),null
)
)
)</formula>
        <label>PPC Estimate</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
