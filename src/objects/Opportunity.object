    <fields>
        <fullName>Is_Survey_Eligible_for_RDS_Auto__c</fullName>
        <description>ESPCRMINT-382,ESPSFDCQI-9573</description>
        <externalId>false</externalId>
        <formula>AND(
    Amount_In_USD__c &gt; 15000,
    OR(
        AND(
            OR(
                ISPICKVAL(Line_of_Business__c , &apos;Data Sciences&apos;),
                ISPICKVAL(Line_of_Business__c , &apos;Early Clinical Development&apos;),
                ISPICKVAL(Line_of_Business__c , &apos;Connected Devices&apos;),
                ISPICKVAL(Line_of_Business__c , &apos;Clinical&apos;),
ISPICKVAL(Line_of_Business__c , &apos;Patient &amp; DCT Solutions&apos;)
            ),
            OR (
                ISPICKVAL(Is_this_IQVIA_Biotech__c , &apos;No&apos;),
                AND(
                    ISPICKVAL(Is_this_IQVIA_Biotech__c , &apos;Yes&apos;),
                    OR( 
                        CONTAINS( TEXT(Global_project_unit__c ),&apos;MedTech&apos;),
                        CONTAINS( TEXT(Global_project_unit__c ),&apos;Biotech&apos;)
                    )
                )
            )
        ),
        AND( OR(ISPICKVAL(Line_of_Business__c , &apos;Outcome&apos;),ISPICKVAL(Line_of_Business__c , &apos;RWLP&apos;)), RWE_Products__c &gt; 0, ISPICKVAL(Is_this_IQVIA_Biotech__c , &apos;No&apos;)),
        AND( Hybrid_Study__c ,OR(ISPICKVAL(Line_of_Business__c , &apos;Outcome&apos;), Hybrid_Products__c &gt; 0)),
        AND(ISPICKVAL(Line_of_Business__c , &apos;Commercial Solutions&apos;), CSMS_Product__c &gt; 0)
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Survey Eligible for RDS Auto</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Survey_Eligible_for_RDS_Manual__c</fullName>
        <description>ESPCRMINT-380,ESPSFDCQI-9573</description>
        <externalId>false</externalId>
        <formula>AND(
Amount_In_USD__c &gt; 15000,
OR(
    AND(ISPICKVAL(Line_of_Business__c , &apos;Data Sciences&apos;),  Medical_Writing_Products__c &gt; 0),
    AND(ISPICKVAL(Line_of_Business__c , &apos;Safety &amp; Med Info&apos;)),
    OR(ISPICKVAL(Line_of_Business__c , &apos;Clinical&apos;),
       ISPICKVAL(Line_of_Business__c , &apos;Data Sciences&apos;),
       ISPICKVAL(Line_of_Business__c , &apos;Early Clinical Development&apos;),
       ISPICKVAL(Line_of_Business__c , &apos;Connected Devices&apos;),
       ISPICKVAL(Line_of_Business__c , &apos;Regulatory &amp; Drug Development Solutions&apos;),
       ISPICKVAL(Line_of_Business__c , &apos;Patient &amp; DCT Solutions&apos;),
	   ISPICKVAL(Line_of_Business__c , &apos;Health Engagement &amp; Communication&apos;)),
    AND(ISPICKVAL(Line_of_Business__c , &apos;Commercial Solutions&apos;), CSMS_Product__c &gt; 0),
    AND( OR (ISPICKVAL(Line_of_Business__c , &apos;Outcome&apos;),ISPICKVAL(Line_of_Business__c , &apos;RWLP&apos;)), RWE_Products__c &gt; 0),
   AND( Hybrid_Study__c ,OR(ISPICKVAL(Line_of_Business__c , &apos;Outcome&apos;), Hybrid_Products__c &gt; 0))
    )
)</formula>
