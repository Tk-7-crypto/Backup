    <validationRules>
        <fullName>SS_VR001_CRM_StatusValidation01</fullName>
        <active>true</active>
        <description>ESPSFDCQI-16339</description>
        <errorConditionFormula>AND(
    NOT(ISPICKVAL(Status__c, &apos;FQ Received&apos;)),
    ISBLANK(PRIORVALUE(FQ_Received__c)),
    NOT(ISBLANK(FQ_Received__c)),
    
    ISBLANK(FQ_Submitted__c),
    ISBLANK(PSV_Requested__c),
    ISBLANK(PSV_Scheduled_for__c),
    ISBLANK(PSV_Completed__c),
    PSV_Waived__c == FALSE,
    ISBLANK(Site_Selection_Notification__c),
    $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please update the Status to FQ Received.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SS_VR002_CRM_StatusValidation02</fullName>
