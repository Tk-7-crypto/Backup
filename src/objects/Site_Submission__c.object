        ISPICKVAL(Status__c, &apos;Selected - Withdrawn (Sponsor/CRO)&apos;),
        ISPICKVAL(Status__c, &apos;Awarded - Withdrawn (Avacare)&apos;),
        ISPICKVAL(Status__c, &apos;Awarded - Withdrawn (Sponsor/CRO)&apos;),
        ISPICKVAL(Status__c, &apos;Awarded - Study Terminated (Sponsor)&apos;)
    ),
    ISBLANK(Site_Selection_Notification__c),
    $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
)</errorConditionFormula>
