    <validationRules>
        <fullName>Only_LineManager_can_create_skill_Rating</fullName>
        <active>true</active>
        <description>OWF-ESPSFDCQI-5261 - Only Line manager of any user can create skill certification rating for the related contacts with triage manager permission set.</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
ISPICKVAL(pse__Skill_Certification__r.pse__Type__c , &quot;Grade Level&quot;),
NOT(OR($Permission.OWF_Triage_Manager,
pse__Resource__r.Salesforce_User__r.ManagerId == $User.Id,$Profile.Name == &quot;System Administrator&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Only your line manager(with triage manager permissions) can create/edit grade level skill certification records. Please contact your line manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR01_OWF_PreventSCR_Create_Edit</fullName>
        <active>true</active>
        <description>OWF - MC- ESPSFDCQI-2272- Validates that User can create Skill Certification Ratings for its own Contact.</description>
        <errorConditionFormula>AND(
	$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = FALSE,
	$Profile.Name &lt;&gt; &apos;System Administrator&apos;,
	AND(
	NOT($Permission.Create_And_Edit_Skills_And_Certification),
	AND(
	NOT($Permission.Create_and_Edit_All_Skill_Certification_Ratings),
	NOT($User.FirstName == pse__Resource__r.FirstName &amp;&amp; $User.LastName == pse__Resource__r.LastName)
	)
	)
	)</errorConditionFormula>
        <errorMessage>You can only create Skill Certification Ratings for your own Contact. If you continue to have issues please
        contact your Triage Manager for support.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR02_OWF_PreventSCR_Create</fullName>
        <active>true</active>
        <description>Rating field is required when trying to save a new skill/certification record</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,OR(ISPICKVAL(pse__Rating__c, &quot;None&quot;),ISPICKVAL(pse__Rating__c, &apos;&apos;))
        ,$Permission.OWF_Sales_User_with_PSA_Access = TRUE)</errorConditionFormula>
        <errorMessage>Please select a Rating.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR03_ValidateRFPRanking_GradeLevel</fullName>
        <active>false</active>
        <description>OWF-ESPSFDCQI-5261-Validate RFP ranking for Grade Level</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
        ISPICKVAL(pse__Skill_Certification__r.pse__Type__c, &apos;Grade Level&apos;), ISBLANK(TEXT(RFP_Ranking__c)))</errorConditionFormula>
        <errorMessage>RFP Ranking can not be blanked for Grade Level Skills</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR04_PreventRFPRanking_NonGradeLevel</fullName>
        <active>false</active>
        <description>OWF-ESPSFDCQI-5261-Prevent RFP ranking for Non Grade Level</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
        !ISPICKVAL(pse__Skill_Certification__r.pse__Type__c, &apos;Grade Level&apos;), !ISBLANK(TEXT(RFP_Ranking__c)))</errorConditionFormula>
        <errorMessage>RFP Ranking can not be filled for Non Grade Level Skills</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR05_Only_Active_ResourceCanAddSkll</fullName>
        <active>true</active>
        <description>OWF-PSA-1843-Only a resource that has active salesforce user can have Skill</description>
        <errorConditionFormula>AND(
        $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
        OR(pse__Resource__r.pse__Salesforce_User__c==NULL,
        pse__Resource__r.pse__Salesforce_User__r.IsActive == FALSE) 
        )</errorConditionFormula>
        <errorDisplayField>pse__Resource__c</errorDisplayField>
        <errorMessage>Resource should have an Active Salesforce User</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR06_ValidateTriageRating_TriagLevel</fullName>
        <active>false</active>
        <description>OWF-IQVIAPSA2169 Validating Triage Rating for Triage level</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
        ISPICKVAL(pse__Skill_Certification__r.pse__Type__c, &apos;Triage Level&apos;),ISBLANK(TEXT(Triage_Level__c)))</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Triage Level can not be blanked for Triage Level skill</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR07_ValidateTriage_Non_TrgLvl</fullName>
        <active>true</active>
        <description>OWF-IQVIAPSA-2169 Validating Triage raitng for Non Triage level skill</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
        !ISPICKVAL(pse__Skill_Certification__r.pse__Type__c, &apos;Triage Level&apos;), !ISBLANK(TEXT(Triage_Level__c)))</errorConditionFormula>
        <errorMessage>Triage Level can not be filled for Non Triage Level Skill</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_VR08_ValidateRatingField</fullName>
        <active>true</active>
        <description>Rating option will be valid for OWF user from 1 to 5</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
BEGINS(pse__Resource__r.pse__Group__r.Name, &apos;OWF&apos;),    
NOT(OR(
(ISBLANK(TEXT( pse__Rating__c ))),
ISPICKVAL( pse__Rating__c , &apos;1 - Limited Exposure&apos;),
ISPICKVAL( pse__Rating__c , &apos;2 - Some Familiarity&apos;),
ISPICKVAL( pse__Rating__c , &apos;3 - Comfortable&apos;),
ISPICKVAL( pse__Rating__c , &apos;4 - Strong&apos;),
ISPICKVAL( pse__Rating__c , &apos;5 - Expert&apos;))))</errorConditionFormula>
        <errorDisplayField>pse__Rating__c</errorDisplayField>
        <errorMessage>Please choose values 1 to 5 in Rating</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Check_Resource</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEN(pse__Resource__c) &gt;  0,  NOT( pse__Resource__r.pse__Is_Resource__c))</errorConditionFormula>
        <errorDisplayField>pse__Resource__c</errorDisplayField>
        <errorMessage>Resource lookup must be set to an Contact with Is Resource = true</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
