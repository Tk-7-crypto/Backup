    <validationRules>
        <fullName>CPQ_Validation_Can_Not_Create_Quote</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(), OR(ISBLANK( Related_Opportunity__c ), ISBLANK(TEXT(GBU__c)), ISBLANK( Pricing_Tools__c), ISBLANK( Related_Account__c)), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE)</errorConditionFormula>
        <errorMessage>You cannot create a Quote from here.Please contact the support team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPQ_Validation_On_AstraZeneca_Fields_I</fullName>
        <active>true</active>
        <description>CPQ-IQ-LC-12927: To ensure that if Discount Applied to Budget % Field is filled than the value of Actual Discount Value field should not be blank.</description>
        <errorConditionFormula>
            AND(
                AND(
                    OR(
                        ISPICKVAL(GBU__c, &quot;RDS SF&quot;),
                        ISPICKVAL(GBU__c, &quot;RDS Non-SF&quot;)
                    ),
                    OR(
                        ISPICKVAL(Quote_Type__c, &quot;Project Specific&quot;),
                        ISPICKVAL(Quote_Type__c, &quot;Change Order&quot;)
                    ),
                    AND(
                        NOT(ISNULL(Discount_Applied_to_Budget__c)),
                        ISBLANK(Actual_Discount_Value__c)
                    )
                ),
                $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
            )
        </errorConditionFormula>
        <errorDisplayField>Actual_Discount_Value__c</errorDisplayField>
        <errorMessage>Please enter a value for &quot;Actual Discount Value&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPQ_Validation_On_AstraZeneca_Fields_II</fullName>
        <active>true</active>
        <description>CPQ-IQ-LC-12927: To ensure that if Actual Discount Value Field is filled than the value of Discount Applied to Budget % field should not be blank.</description>
        <errorConditionFormula>
		    AND(
                AND(
                    OR(
                        ISPICKVAL(GBU__c, &quot;RDS SF&quot;),
                        ISPICKVAL(GBU__c, &quot;RDS Non-SF&quot;)
                    ),
                    OR(
                        ISPICKVAL(Quote_Type__c, &quot;Project Specific&quot;),
                        ISPICKVAL(Quote_Type__c, &quot;Change Order&quot;)
                    ),
                    AND(
                        ISNULL(Discount_Applied_to_Budget__c),
                        NOT(ISBLANK(Actual_Discount_Value__c))
                    )
                ),
                $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
            )
        </errorConditionFormula>
        <errorDisplayField>Discount_Applied_to_Budget__c</errorDisplayField>
        <errorMessage>Please enter a value for &quot;Discount Applied to Budget %&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPQ_Validation_On_Field_Bid_Sent_Date</fullName>
        <active>true</active>
        <description>CPQ-IQ-LC-12927: Validation rule for Bid Sent Date field, to entry at the Generated Stage.</description>
        <errorConditionFormula>
            AND(
                ISPICKVAL(GBU__c, &quot;RDS SF&quot;),
                OR(
                    ISPICKVAL(Quote_Type__c, &quot;Initial&quot;),
                    ISPICKVAL(Quote_Type__c, &quot;Re-bid&quot;)
                ),
                IF(
                    AND(
                        ISCHANGED(Bid_Sent_Date__c),
                        ISPICKVAL(Approval_Stage__c, &quot;Draft&quot;)
                    ),
                    TRUE,
                    FALSE
                ),
                $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
            )
        </errorConditionFormula>
        <errorMessage>You can only enter Bid sent date at the Generated Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPQ_Validation_On_Status_Field</fullName>
        <active>true</active>
        <description>CPQ-IQ-LC-12927: User should not be able to update any other status except Rejected once CNF/Ballpark is accepted,</description>
        <errorConditionFormula>
            AND(
                OR(
                    ISPICKVAL(GBU__c, &quot;RDS SF&quot;),
                    ISPICKVAL(GBU__c, &quot;RDS Non-SF&quot;)
                ),
                OR(
                    ISPICKVAL(Quote_Type__c, &quot;CNF&quot;),
                    ISPICKVAL(Quote_Type__c, &quot;Ballpark&quot;)
                ),
                ISPICKVAL(PRIORVALUE(Status__c), &quot;Accepted&quot;),
                ISCHANGED(Status__c),
                NOT(ISPICKVAL(Status__c, &quot;Rejected&quot;)),
                $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
            )
        </errorConditionFormula>
        <errorMessage>You can not perform this action on an accepted quote.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPQ_Validation_Rule_On_Bid_Sent</fullName>
        <active>true</active>
        <description>CPQ-IQ-LC-12927: To make the &apos;Number of Scenarios&apos; field mandatory on Quote when Bid Sent = Yes</description>
        <errorConditionFormula>
            AND(
                AND(
                    ISPICKVAL(GBU__c, &quot;RDS SF&quot;),
                    OR(
                        ISPICKVAL(Quote_Type__c, &quot;Initial&quot;),
                        ISPICKVAL(Quote_Type__c, &quot;Re-bid&quot;)
                    ),
                    ISPICKVAL(Bid_Sent__c, &quot;Yes&quot;),
                    ISBLANK(Final_Number_of_Scenarios__c)
                ),
                $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
            )
        </errorConditionFormula>
        <errorMessage>Please enter a value for: Number of Scenarios</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPQ_Validation_To_CNF_Approval_Date</fullName>
        <active>true</active>
        <description>CPQ-IQ-LC-12927: If User selects status as Rejected/Accepted/Closed for Internal Purpose and Approval/Rejected/Completed Date is blank for CNF or Ballpark quote record then displays a validation message.</description>
        <errorConditionFormula>
            AND(
                OR(
                    ISPICKVAL(Quote_Type__c, &quot;CNF&quot;),
                    ISPICKVAL(Quote_Type__c, &quot;Ballpark&quot;)
                ),
                OR(
                    ISPICKVAL(Status__c, &quot;Rejected&quot;),
                    ISPICKVAL(Status__c, &quot;Accepted&quot;),
                    ISPICKVAL(Status__c, &quot;Closed for Internal Purposes&quot;)
                ),
                ISNULL(Approval_Rejected_Completed_Date__c),
                ISCHANGED(Status__c),
                OR(
                    ISPICKVAL(GBU__c, &quot;RDS SF&quot;),
                    ISPICKVAL(GBU__c, &quot;RDS Non-SF&quot;)
                ),
                $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
            )
        </errorConditionFormula>
        <errorMessage>Please enter Approval/Rejected/Completion Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPQ_Validation_Validate_ICF_Short_Only</fullName>
        <active>false</active>
        <errorConditionFormula>IF( OR(ICF_Short_Only__c &lt; 0 , ICF_Short_Only__c &gt; ICFs__c)  , true, false)</errorConditionFormula>
        <errorMessage>Please enter a value for the &quot;ICF# (Short Only)&quot; field that is greater than zero and less than the &quot;ICF# (Main &amp; Short)&quot; value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scenario_Number_cannot_be_in_decimal</fullName>
        <active>true</active>
        <errorConditionFormula>MOD(Scenario__c, 1) != 0</errorConditionFormula>
        <errorDisplayField>Scenario__c</errorDisplayField>
        <errorMessage>Decimal values are not allowed in Scenario Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Upfront_Amount</fullName>
        <active>true</active>
        <description>This rule validates the conditions on Upfront Amount field.</description>
        <errorConditionFormula>
        AND( 
            INCLUDES(Pricing_Tools__c, &quot;Q2 Solutions&quot;), 
            NOT(ISBLANK(Upfront_Payment_Amount__c)), 
            OR(
                ISNEW(), 
                ISCHANGED(Upfront_Payment_Amount__c)
            ), 
            OR(
                ISPICKVAL(Related_Opportunity__r.Full_Service__c, &apos;Yes&apos;), 
                NOT(ISBLANK(Related_Opportunity__r.Parent_Opportunity__c)), 
                ISPICKVAL(Related_Bid__r.Bid_Type__c, &apos;Change Order&apos;)
            )
        )
        </errorConditionFormula>
        <errorMessage>Upfront payment Criteria not satisfied -
            1: Quote opportunity is full service.
            2: Quote opportunity has Parent opportunity.
            3: Quote Bid type is Change Order.
        </errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Upfront_Amount_cannot_be_Negative</fullName>
        <active>true</active>
        <description>Validation Rule to ensure that the value in Upfront Amount field should not be Negative.</description>
        <errorConditionFormula>AND(INCLUDES(Pricing_Tools__c, &apos;Q2 Solutions&apos;), NOT(ISBLANK(Upfront_Payment_Amount__c)), Upfront_Payment_Amount__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>Upfront_Payment_Amount__c</errorDisplayField>
        <errorMessage>Upfront amount cannot be negative.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>Create_Quote</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Create Quote</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>/lightning/cmp/c__LXC_CPQ_CreateQuoteButton?c__recordId={!Opportunity.Id}</url>
    </webLinks>
</CustomObject>
