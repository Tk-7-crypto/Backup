   IF(Apttus_Config2__OptionId__r.ProductCode = &apos;OCEP_AO_RE_IAVM&apos;, Apttus_Config2__Quantity__c * 45,
   IF(Apttus_Config2__OptionId__r.ProductCode = &apos;OCEP_AO_ADA&apos; &amp;&amp; Apttus_Config2__Quantity__c &gt; 5, (Apttus_Config2__Quantity__c - 5) * 1440,  
   IF(ISPICKVAL(Apttus_Config2__ChargeType__c, &apos;Subscription Fee&apos;), Apttus_Config2__ExtendedPrice__c * 0.35, 
   IF(ISPICKVAL(Apttus_Config2__ChargeType__c, &apos;Add-on Price&apos;), Apttus_Config2__ExtendedPrice__c * 0.8,
   IF(ISPICKVAL(Apttus_Config2__ChargeType__c, &apos;Implementation Fee&apos;), Apttus_Config2__ExtendedPrice__c / 2.5,
