    <validationRules>
        <fullName>SS_01_SearchStrategygotClientApproval</fullName>
        <active>true</active>
        <description>IQVIAPSA-4652</description>
        <errorConditionFormula>AND(Date_Search_Strategy_got_Client_Approval__c &gt; TODAY(), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == False)</errorConditionFormula>
        <errorMessage>&apos;Date Search Strategy got Client Approval&apos; cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SS_02_DateAnnualValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNULL( Date_Annual_Search_Strategy_Due__c ), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Date_Annual_Search_Strategy_Due__c</errorDisplayField>
        <errorMessage>Date Annual Search Strategy Due&apos; field is a required field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SS_04_Search_Strategy_Created</fullName>
        <active>false</active>
        <errorConditionFormula>AND(OR (ISPICKVAL(LR_Project_Overview__r.Search_Strategy_Created__c,&apos;No&apos;), ISPICKVAL(LR_Project_Overview__r.Search_Strategy_Created__c,&apos;Client Responsible&apos;), ISPICKVAL(LR_Project_Overview__r.Search_Strategy_Created__c,&apos;N/A&apos;)), ISNEW() ,$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False)</errorConditionFormula>
        <errorMessage>When Search Strategy is set to “No, &apos;N/A&apos; or Client Responsible” in the Project overview section you cannot create a Search Strategy record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SS_05_CheckDataFromLR</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == False,
OR(
ISBLANK(Account__c),
ISBLANK(Product__c),
ISBLANK(Project__c),
ISPICKVAL(LR_Type__c, &apos;&apos;)
)
)</errorConditionFormula>
        <errorMessage>You cannot create a new search strategy record because Project, Account, Product and LR Type is blank on the LR project overview record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SS_06_Search_Strategy_Not_Created</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  
   OR(ISPICKVAL( LR_Project_Overview__r.Status__c , &apos;Completed&apos;),
      ISPICKVAL( LR_Project_Overview__r.Status__c , &apos;Cancelled&apos;)
     ),  $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False)</errorConditionFormula>
        <errorMessage>You cannot create new search strategy records when status of LR Project overview is marked as Completed or Cancelled</errorMessage>
    </validationRules>
