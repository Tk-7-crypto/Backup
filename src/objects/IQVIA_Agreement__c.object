        <externalId>false</externalId>
        <formula>IF(AND(
OR($RecordType.DeveloperName == &quot;USBU&quot;,$RecordType.DeveloperName == &quot;MSA&quot;),
ISPICKVAL(Status_Category__c, &quot;In Signatures&quot;),
ISPICKVAL(Status__c, &quot;Ready for Signatures&quot;),
NOT(ISBLANK(Docusign_Account_Id__c)),
OR(
ISBLANK(DocuSign_Envelope_Id__c),
AND(
NOT(ISBLANK(DocuSign_Envelope_Id__c)),
OR(
DocuSign_Envelope_Id__r.Status__c  == &quot;Completed&quot;,
DocuSign_Envelope_Id__r.Status__c == &quot;Cancelled&quot;,
DocuSign_Envelope_Id__r.Status__c == &quot;Declined&quot;,
DocuSign_Envelope_Id__r.Status__c == &quot;Voided&quot;)))),
HYPERLINK(&quot;/lightning/cmp/c__LXC_CLM_DocuSignSendForESignature?c__recordId=&quot;+Id+&quot;&amp;c__accId=&quot;+Docusign_Account_Id__c,
IMAGE(&quot;/resource/SendeSigantureAP&quot;, &quot;Send For eSignature&quot;) , &quot;_self&quot;),&quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
