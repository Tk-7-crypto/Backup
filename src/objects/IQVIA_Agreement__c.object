    <fields>
        <fullName>Check_eSignature_Status__c</fullName>
        <description>CLM-MC-CLMCMA-18411 - formula field for check for eSignature</description>
        <externalId>false</externalId>
        <formula>IF(AND(
OR($RecordType.DeveloperName == &quot;USBU&quot;,$RecordType.DeveloperName == &quot;MSA&quot;),
ISPICKVAL(Status_Category__c, &quot;In Signatures&quot;),
NOT(ISBLANK(Docusign_Account_Id__c)),
NOT(ISBLANK(DocuSign_Envelope_Id__c)),
DocuSign_Envelope_Id__r.Status__c != &quot;Completed&quot;,
DocuSign_Envelope_Id__r.Status__c != &quot;Cancelled&quot;,
DocuSign_Envelope_Id__r.Status__c != &quot;Declined&quot;,
DocuSign_Envelope_Id__r.Status__c != &quot;Voided&quot;),
HYPERLINK(&quot;/lightning/cmp/c__LXC_CLM_checkEsignatureStatus?c__recordId=&quot;+Id, 
IMAGE(&quot;/resource/CheckeSignatureStatusAP&quot;,&quot;Check eSignature Status&quot;), &quot;_self&quot;),&quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Check eSignature Status</label>
