    <validationRules>
        <fullName>DOF_VR01_OWF_First_dayOff_MustBeInFuture</fullName>
        <active>true</active>
        <description>OWF- MC-ESPSFDCQI-2249-Validates that the First day off requested must be in the future.</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE, 
OR ( 
ISNEW(), 
ISCHANGED( First_Day_Off__c)), 
First_Day_Off__c &lt; Today(),
$Profile.Name &lt;&gt; &apos;System Administrator&apos;
)</errorConditionFormula>
        <errorDisplayField>First_Day_Off__c</errorDisplayField>
        <errorMessage>First day off requested must be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOF_VR02_OWF_LastDay_greaterThanFirstDay</fullName>
        <active>true</active>
        <description>OWF- MC-ESPSFDCQI-2249-Validates that the The Last Day off requested must be greater than or equal to the First Day of requested.</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE, Last_Day_Off__c &lt; First_Day_Off__c)</errorConditionFormula>
        <errorDisplayField>Last_Day_Off__c</errorDisplayField>
        <errorMessage>Last day off needs to be greater than or equal to the first day off.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOF_VR03_OWF_CanNotChange_CancelledAssig</fullName>
        <active>true</active>
        <errorConditionFormula>AND
($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
NOT(ISNEW()),
ISCHANGED(Status__c),
PRIORVALUE(Status__c)==&apos;Cancelled&apos;
)</errorConditionFormula>
        <errorMessage>Status of Cancelled Days Off cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOF_VR04_OWF_CanChangeStatusOnly</fullName>
        <active>true</active>
        <description>OWF - ESPSFDCQI-5037 - Record Creator can only update the status field.</description>
        <errorConditionFormula>AND 
($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE, 
NOT(ISNEW()),
OR(ISCHANGED(First_Day_Off__c),
ISCHANGED(Last_Day_Off__c),
ISCHANGED( Assignment_Type__c ),
ISCHANGED(  Assignment__c  ),
ISCHANGED(  Request_Type__c  )
 ),
AND(NOT($Permission.OWF_Triage_Manager),NOT( $Profile.Name == &apos;System Administrator&apos;))
)</errorConditionFormula>
        <errorMessage>Only Status can be updated of already created record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOF_VR05_OnlyRecordCreaterCanUpdate</fullName>
        <active>true</active>
        <description>OWF - ESPSFDCQI-5037 - Only the Owner can update the record.</description>
        <errorConditionFormula>AND
($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
OR( ISNEW() , NOT( ISNEW() )),
AND( $User.Id &lt;&gt;   Employee__r.pse__Salesforce_User__r.Id, NOT( $Permission.OWF_Triage_Manager), $Profile.Name &lt;&gt; &apos;System Administrator&apos; )
)</errorConditionFormula>
        <errorMessage>Only the Employee or the Triage Manager can Create / Update the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOF_VR06_Only_Active_Resource_DaysOFF</fullName>
        <active>true</active>
        <description>OWF-PSA-1843-Only a resource that has active salesforce user can have a Days Off</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
Employee__r.pse__Salesforce_User__c == NULL
)</errorConditionFormula>
        <errorDisplayField>Employee__c</errorDisplayField>
        <errorMessage>Employee/Contact should have an Active Salesforce User</errorMessage>
    </validationRules>
