    <validationRules>
        <fullName>CheckCustomPermission</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(CONTAINS($Profile.Name,&apos;System Administrator&apos;) ),
NOT($Profile.Name = &apos;IQVIA Salesforce Platform Support&apos;),
OR(NOT($Permission.TECHNO_Case_Record_Type_Contact_Fields_Access) , NOT( $Permission.R_D_Case_Record_Type_Study_Activity_Objects_Access)),
OR( AND(NOT($Permission.TECHNO_Case_Record_Type_Contact_Fields_Access), NOT( $Permission.R_D_Case_Record_Type_Study_Activity_Objects_Access)),
AND($Permission.TECHNO_Case_Record_Type_Contact_Fields_Access, ISPICKVAL(Pillar__c, &apos;R&amp;D&apos;)),
AND($Permission.R_D_Case_Record_Type_Study_Activity_Objects_Access, ISPICKVAL(Pillar__c, &apos;Technology Solutions&apos;))))</errorConditionFormula>
        <errorMessage>Can&apos;t save this record. Either you do not have right access or you are not selecting correct pillar.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_end_date_greater_than_start_date</fullName>
        <active>true</active>
        <errorConditionFormula>(End_Date__c &lt;  Start_Date__c)  || (End_Date__c &lt; TODAY())</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End date can&apos;t be a past date or  less than start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_inserting_past_date</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Start date can&apos;t be past date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_saving_contact_with_email</fullName>
        <active>true</active>
        <errorConditionFormula>ContactId__c != null &amp;&amp;  Email_Adresss__c != null &amp;&amp;  Parent__c ==null</errorConditionFormula>
        <errorMessage>You cannot insert single contact and Bulk email addresses at the same time.

Please review the fields accordantly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Saving_record_without_contact</fullName>
        <active>true</active>
        <errorConditionFormula>ContactId__c == null &amp;&amp;  Email_Adresss__c == null
&amp;&amp;  ISPICKVAL(Pillar__c, &apos;Technology Solutions&apos;)</errorConditionFormula>
        <errorMessage>Please fill contact or Email Addresses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_field_update</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(ContactId__c ) ||  ISCHANGED(Email_Adresss__c) || ISCHANGED(Pillar__c)</errorConditionFormula>
        <errorMessage>update is allowed for only message and start/end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_saving_record_without_los</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Pillar__c, &apos;R&amp;D&apos;),
ISBLANK(Queue__c),ISPICKVAL(LOS__c, &apos;&apos;)	
)</errorConditionFormula>
        <errorMessage>Please select LOS  or Queue before saving the record.</errorMessage>
    </validationRules>
