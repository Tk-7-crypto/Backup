        <description>PEP-ACN-PRM-89-Partner Role</description>
        <externalId>false</externalId>
        <formula>IF(TEXT(Salesforce_User_ID__r.UserRole.PortalRole)=&apos;Manager&apos;, &apos;Manager&apos;, &apos;User&apos;)</formula>
        <label>Partner Role</label>
        <required>false</required>
