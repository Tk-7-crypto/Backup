    </fields>
    <fields>
        <fullName>Partner_Type__c</fullName>
        <description>PEP-ACN-PRM-89-Partner Type</description>
        <externalId>false</externalId>
        <formula>LEFT(IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;Integrator Partner&apos;) , &apos;Integrator Partner;&apos;, NULL) + IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;Data Partner&apos;) , &apos;Data Partner;&apos;, NULL) + IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;Tech Partner&apos;) , &apos;Tech Partner;&apos;, NULL) + IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;CLM Agency&apos;) , &apos;CLM Agency &apos;, NULL),LEN(IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;Integrator Partner&apos;) , &apos;Integrator Partner;&apos;, NULL) + IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;Data Partner&apos;) , &apos;Data Partner;&apos;, NULL) + IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;Tech Partner&apos;) , &apos;Tech Partner;&apos;, NULL) + IF( INCLUDES( Salesforce_User_ID__r.Contact.Partner_Type__c, &apos;CLM Agency&apos;) , &apos;CLM Agency &apos;, NULL))-1)</formula>
        <label>Partner Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
