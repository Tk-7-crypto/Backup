    <validationRules>
        <fullName>OLI_VR01_CRM_Prd_Start_Less_than_Prd_End</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQi-37 - Validates that the custom fields called Product End Date does not come before another custom field called Product Start Date.</description>
        <errorConditionFormula>Product_Start_Date__c &gt; Product_End_Date__c &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE</errorConditionFormula>
        <errorDisplayField>Product_Start_Date__c</errorDisplayField>
        <errorMessage>Product End Date cannot be before Product Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR02_CRM_Dlvy_St_Less_than_Dlvy_End</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-37 - Validates that the custom fields called Delivery End Date does not come before another custom field called Delivery Start Date.</description>
        <errorConditionFormula>Delivery_Start_Date__c &gt; Delivery_End_Date__c &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE</errorConditionFormula>
        <errorDisplayField>Delivery_Start_Date__c</errorDisplayField>
        <errorMessage>Delivery End Date cannot be before Delivery Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR03_CRM_QuantityEqualsOne</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-1126 - Validation that ensures the Quantity field always equals 1</description>
        <errorConditionFormula>OR( Quantity &gt; 1 , Quantity &lt; 1)</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>A products quantity must always be 1. Please review and set the quantity to 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR04_CRM_Sales_Price_Required</fullName>
        <active>false</active>
        <description>CRM-MC-ESPSFDCQI-1295 - Sales Price is required when creating Opportunity Products.</description>
        <errorConditionFormula>UnitPrice = 0
&amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Sales Price is required when adding products to an opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR05_CRM_Sale_Type_Required</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-1297 - Sale Type is required when creating Opportunity Products.</description>
        <errorConditionFormula>ISBLANK(TEXT(Sale_Type__c))
&amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE</errorConditionFormula>
        <errorDisplayField>Sale_Type__c</errorDisplayField>
        <errorMessage>Sale Type is required when adding products to an opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR06_CRM_Revenue_Type_Required</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-1297 - Revenue Type is required when creating Opportunity Products.</description>
        <errorConditionFormula>ISBLANK(TEXT(Revenue_Type__c))
&amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE</errorConditionFormula>
        <errorDisplayField>Revenue_Type__c</errorDisplayField>
        <errorMessage>Revenue Type is required when adding products to an opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR07_CRM_Sales_Price_Not_Negative</fullName>
        <active>false</active>
        <description>CRM-MC-ESPSFDCQI-1908 - Sales Price cannot be negative when creating Opportunity Products.</description>
        <errorConditionFormula>UnitPrice &lt; 0
&amp;&amp; 
Product2.Material_Type__c != &apos;ZQUI&apos; &amp;&amp;
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Sales Price cannot be negative when adding products to an opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR08_CRM_Min_Lic_Less_than_Max_Lic</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-2645 - Validates that the min licenses should always be less than max licenses</description>
        <errorConditionFormula>(ISBLANK(Max_Licenses__c) &amp;&amp; NOT(ISBLANK(Min_Licenses__c))) || (Min_Licenses__c &gt; Max_Licenses__c)</errorConditionFormula>
        <errorDisplayField>Max_Licenses__c</errorDisplayField>
        <errorMessage>Min Licenses should always be less than max licenses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR09_CRM_Cant_Updt_Sales_Price_Dirct</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQi-2942 - Sales price cannot be edited directly. Please establish revenue schedules for this line item and the sales price will be calculated accordingly.</description>
        <errorConditionFormula>AND (
    NOT(ISNEW()),
    ISCHANGED(UnitPrice),
    HasRevenueSchedule == FALSE,
    UnitPrice != 0,
    OR (
        Product_Material_Type_Text__c = &apos;ZREP&apos;,
        Product_Material_Type_Text__c = &apos;ZPUB&apos;
    ),
    $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Sales price cannot be edited directly. Please establish revenue schedules for this line item and the sales price will be calculated accordingly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR10_CRM_Stop_Insert_Update</fullName>
        <active>false</active>
        <description>CRM-MC-ESPSFDCQI-3849 - Stop users from creating or updating opportunity line items</description>
        <errorConditionFormula>$Setup.Mulesoft_Integration_Control__c.ReadOnlyMode__c</errorConditionFormula>
        <errorMessage>Currently this Application is offline for Data migration and will be up soon. Sorry for any inconvenience. CRM Data Migration Team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR11_CRM_Product_End_Date_Validation</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051, Product End Date cannot be more than 10 years in the future</description>
        <errorConditionFormula>NOT(CONTAINS(UPPER($Profile.Name) , &apos;SYSTEM ADMINISTRATOR&apos;)) &amp;&amp; ( Product_End_Date__c - TODAY() ) &gt; 10*365.25  &amp;&amp;  Product2.Material_Type__c != &apos;ZREP&apos; &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE &amp;&amp; $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c</errorConditionFormula>
        <errorDisplayField>Product_End_Date__c</errorDisplayField>
        <errorMessage>Product End Date cannot be more than 10 years in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR12_CRM_Product_Start_Date_Validati</fullName>
        <active>false</active>
        <description>ESPSFDCQI-4051, Product Start Date cannot be more than 5 years in the future</description>
        <errorConditionFormula>NOT(CONTAINS(UPPER($Profile.Name) , &apos;SYSTEM ADMINISTRATOR&apos;)) &amp;&amp; ( Product_Start_Date__c - TODAY() ) &gt; 5*365.25 &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE  &amp;&amp; $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c</errorConditionFormula>
        <errorDisplayField>Product_Start_Date__c</errorDisplayField>
        <errorMessage>Product Start Date cannot be more than 5 years in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR13_CRM_Delivery_date_before_ProdED</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051,</description>
        <errorConditionFormula>!CONTAINS(UPPER($Profile.Name), &quot;SYSTEM ADMINISTRATOR&quot;) &amp;&amp; Delivery_Date__c &lt; Product_End_Date__c &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE  &amp;&amp; $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c</errorConditionFormula>
        <errorDisplayField>Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery date cannot be earlier than Product end Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR14_CRM_Invoice_Date_Validation</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051, Prevent invoice date from being more than 5 years in the future</description>
        <errorConditionFormula>( Invoice_Date__c - TODAY() ) &gt; 5*365.25  &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE  &amp;&amp; $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c</errorConditionFormula>
        <errorMessage>Invoice date cannot be more than 5 years in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR15_CRM_Opportunity_Product_Validat</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051, Product Period Date Validation</description>
        <errorConditionFormula>if((Product_End_Date__c &lt; Product_Start_Date__c ) &amp;&amp; (!CONTAINS(UPPER($Profile.Name), &quot;SYSTEM ADMINISTRATOR&quot;)) ,true,false) &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE  &amp;&amp; $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c</errorConditionFormula>
        <errorDisplayField>Product_Start_Date__c</errorDisplayField>
        <errorMessage>Product Start Date can&apos;t be greater than Product End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR16_CRM_Validation_Billing_date</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051, Billing date cannot be more than 5 years in the future</description>
        <errorConditionFormula>( Billing_Date__c - TODAY() ) &gt; 5*365.25 &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE  &amp;&amp; $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c</errorConditionFormula>
        <errorDisplayField>Billing_Date__c</errorDisplayField>
        <errorMessage>Billing date cannot be more than 5 years in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR17_CRM_Validation_Service_date</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051, Service date cannot be more than 5 years in the future</description>
        <errorConditionFormula>( ServiceDate - TODAY() ) &gt; 5*365.25  &amp;&amp; $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE  &amp;&amp; $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c</errorConditionFormula>
        <errorDisplayField>ServiceDate</errorDisplayField>
        <errorMessage>Service date cannot be more than 5 years in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR18_CRM_Discount_Reason_Blank</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051, Only Operations, System Administrator and Finance profile users are exempted from this validation</description>
        <errorConditionFormula>AND(Discount_Amount_Formula__c &gt; 0,
ISPICKVAL( Discount_Reason__c, &apos;&apos;),
NOT(OR(CONTAINS(UPPER($Profile.Name), &quot;SYSTEM ADMINISTRATOR&quot;),$Permission.IsBNFMDMUser ,AND( CONTAINS(UPPER($Profile.Name),&quot;OPERATIONS&quot;),CONTAINS(UPPER($Profile.Name),&quot;JAPAN&quot;) ))),
OR(ISPICKVAL(Opportunity.StageName ,
&apos;7a. Closed Won&apos;),ISPICKVAL(Opportunity.StageName , &apos;In Hand&apos;)),
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE, $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c 
)</errorConditionFormula>
        <errorDisplayField>Discount_Reason__c</errorDisplayField>
        <errorMessage>Please select a Discount Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR19_CRM_Prevent_surcharge_on_ZPLI_Z</fullName>
        <active>true</active>
        <description>ESPSFDCQI-4051, Prevent_surcharge_on_ZPLI_ZPLU_products</description>
        <errorConditionFormula>AND(Discount_Amount_Formula__c &lt; 0,
( Product2.Item_Category_Group__c == &quot;ZPLI&quot; || Product2.Item_Category_Group__c == &quot;ZPLU&quot;) ,
NOT(OR(CONTAINS(UPPER($Profile.Name), &quot;SYSTEM ADMINISTRATOR&quot;),AND( CONTAINS(UPPER($Profile.Name),&quot;OPERATIONS&quot;),CONTAINS(UPPER($Profile.Name),&quot;JAPAN&quot;) )))
,OR(ISPICKVAL(Opportunity.StageName ,
&apos;6a - Closed Won&apos;),ISPICKVAL(Opportunity.StageName , &apos;In Hand&apos;)),
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE, $Setup.BNF_Settings__c.Enable_IQVIA_BNF_Validation__c
)</errorConditionFormula>
        <errorDisplayField>ListPrice</errorDisplayField>
        <errorMessage>Surcharge is not permitted for ZPLI or ZPLU products</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR20_CRM_Therapy_Area_Required</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-2485-Therapy Area is required when Material Type of Product is ZREP or ZPUB</description>
        <errorConditionFormula>AND (
 OR(PricebookEntry.Product2.Material_Type__c == &apos;ZREP&apos;, 
 PricebookEntry.Product2.Material_Type__c == &apos;ZPUB&apos;),
 ISBLANK(TEXT(Therapy_Area__c)), !$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c
 )</errorConditionFormula>
        <errorMessage>Therapy Area is required for products billed from SAP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_VR21_CRM_Negative_LI_Prod_ComOpsUser</fullName>
        <active>true</active>
        <description>CRM-MC-ESPSFDCQI-3535. Allow the addition of negative sales price value LI products only by commercial operation user.</description>
        <errorConditionFormula>UnitPrice &lt; 0
&amp;&amp;
OR(Product2.Material_Type__c == &apos;ZREP&apos;,Product2.Material_Type__c == &apos;ZPUB&apos;)
&amp;&amp;
$Profile.Name != &apos;Commercial Operations User&apos;
&amp;&amp;
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
&amp;&amp;
NOT($Permission.Commercial_Ops_User)</errorConditionFormula>
        <errorMessage>Please Enter Positive Revenue Amount</errorMessage>
    </validationRules>
    <validationRules>
