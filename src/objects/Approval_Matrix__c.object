        <fullName>Rule_for_mandatory_Approver_1</fullName>
        <active>true</active>
        <description>LC-10411 :Validation on Approval matrix.</description>
        <errorConditionFormula>IF (Process_Type__c = &apos;Approval Process&apos;, IF (Auto_Approve__c = False, ISBLANK(Approver_1__c),  OR(Approver_1__c &lt;&gt; null, Approver_2__c &lt;&gt; null,Approver_3__c &lt;&gt; null,Approver_4__c &lt;&gt; null,Approver_5__c &lt;&gt; null) ), false)</errorConditionFormula>
