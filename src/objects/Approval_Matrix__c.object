    <validationRules>
        <fullName>Validation_Rule_on_Approver</fullName>
        <active>true</active>
        <description>LC-10411 :Validation on Approval matrix.</description>
        <errorConditionFormula>AND(OR(AND(ISBLANK(Approver_2__c ) , (Approver_3__c != null || Approver_4__c != null || Approver_5__c != null)), AND(ISBLANK(Approver_2__c ) || ISBLANK(Approver_3__c ), (Approver_4__c != null || Approver_5__c != null)), AND(ISBLANK(Approver_2__c ) || ISBLANK(Approver_3__c ) || ISBLANK(Approver_4__c ), (Approver_5__c != null))), Process_Type__c = &apos;Approval Process&apos;)</errorConditionFormula>
        <errorMessage>Select approver in ascending order before proceeding</errorMessage>
