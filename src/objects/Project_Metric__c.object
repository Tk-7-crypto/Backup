    <validationRules>
        <fullName>PM_VR01_CannotLastReviewDateInFuture</fullName>
        <active>true</active>
        <description>IQVIAPSA-9850</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
Last_reviewed_Month__c  &gt; Today(),
OR(ISCHANGED( Last_reviewed_Month__c ),  ISNEW())  
)</errorConditionFormula>
        <errorMessage>&apos;Last reviewed Month&apos; cannot be a Future Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_VR02_CannotEditLastReviewDate</fullName>
        <active>true</active>
        <description>IQVIAPSA-9850</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
OR(Retired__c = true, Automated__c = True),OR(ISCHANGED( Last_reviewed_Month__c ), AND( Last_reviewed_Month__c != null,ISNEW()))
)</errorConditionFormula>
        <errorMessage>&apos;Last reviewed Month&apos; can be edited when  Automated  and Retired Date fields are not selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_VR03_PSA_NotAddLastReviewMonthDate</fullName>
        <active>true</active>
        <description>IQVIAPSA-9850</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
OR(Last_reviewed_Month__c &lt; Project__r.pse__Start_Date__c ,  Last_reviewed_Month__c &gt; Project__r.pse__End_Date__c)   
)</errorConditionFormula>
        <errorMessage>&apos;Last reviewed Month&apos;  not to allow  to select a date after or before Project start and end date.</errorMessage>
    </validationRules>
