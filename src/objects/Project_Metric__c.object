        <fullName>Protocol_ID__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>IQVIAPSA-MC-3651: As a PSA user I need the ability to track all tasks/metrics separately for multiple protocols associated to one project code</description>
        <externalId>false</externalId>
        <label>Protocol ID</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>The linked  Protocol is inactive, you cannot edit metrics record linked  with inactive Protocol.</errorMessage>
            <filterItems>
                <field>$Source.Project__c</field>
                <operation>equals</operation>
                <valueField>Project_Target__c.Project__c</valueField>
            </filterItems>
            <filterItems>
                <field>Project_Target__c.Active__c</field>
                <operation>equals</operation>
                <value>True</value>
            </filterItems>
            <filterItems>
                <field>Project_Target__c.Target__r.RecordTypeId</field>
                <operation>equals</operation>
                <value>RDS Protocol</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Project_Target__c</referenceTo>
        <relationshipLabel>Project Metrics</relationshipLabel>
        <relationshipName>Project_Metrics</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
