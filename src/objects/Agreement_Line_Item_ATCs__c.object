    <validationRules>
        <fullName>DataPeriodEnd</fullName>
        <active>true</active>
        <description>Data Period End should not be date before Data Period Start.</description>
        <errorConditionFormula>Data_Period_End__c  &lt;  Data_Period_Start__c</errorConditionFormula>
        <errorDisplayField>Data_Period_End__c</errorDisplayField>
        <errorMessage>Data Period End should not be date before Data Period Start.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Charge_deliver_of_ATC_check</fullName>
        <active>true</active>
        <description>This field is mandatory when Period of Non-Charge Deliver Dates are not Null.</description>
        <errorConditionFormula>AND(OR( NOT( ISBLANK( Period_of_Non_Charge_Deliver_Start__c ) ) ,NOT( ISBLANK( Period_of_Non_Charge_Deliver_End__c ) ) ) ,NOT( Non_Charge_deliver_of_ATC__c) , 
OR(CONTAINS( $Profile.Name , &apos;Japan&apos;) )  )</errorConditionFormula>
        <errorDisplayField>Non_Charge_deliver_of_ATC__c</errorDisplayField>
        <errorMessage>Please check Non-Charge deliver of ATC check box when Non-Charge Deliver Dates are there.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PeriodOfNonChargeDeliverEnd</fullName>
        <active>true</active>
        <description>Period of Non Charge Deliver End should not be date before Period of Non Charge Deliver Start.</description>
        <errorConditionFormula>Period_of_Non_Charge_Deliver_End__c &lt;  Period_of_Non_Charge_Deliver_Start__c</errorConditionFormula>
        <errorDisplayField>Period_of_Non_Charge_Deliver_End__c</errorDisplayField>
        <errorMessage>Period of Non Charge Deliver End should not be date before Period of Non Charge Deliver Start.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Period_of_nonChargeDeliverDates_Null</fullName>
        <active>true</active>
        <description>Period of Non-Charge Deliver Dates are mandatory when Non Charge deliver of ATC is checked.</description>
        <errorConditionFormula>AND( Non_Charge_deliver_of_ATC__c , OR( ISBLANK(Period_of_Non_Charge_Deliver_Start__c ) , ISBLANK( Period_of_Non_Charge_Deliver_End__c ) ) , OR(CONTAINS( $Profile.Name , &apos;Japan&apos;) 
 )
 )</errorConditionFormula>
        <errorMessage>Period of Non-Charge Deliver Dates are mandatory when Non Charge deliver of ATC is set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TerminationDate</fullName>
        <active>true</active>
        <description>Data Period End date should not be less then Termination date (if not blank)</description>
        <errorConditionFormula>AND( NOT ( ISBLANK( Termination_Date__c ) ), Data_Period_End__c &lt; Termination_Date__c )</errorConditionFormula>
        <errorDisplayField>Termination_Date__c</errorDisplayField>
        <errorMessage>Termination Date should not be greater than Data Period End.</errorMessage>
    </validationRules>
