        <fullName>Restrict_TimeUpdate</fullName>
        <active>true</active>
        <errorConditionFormula>(ISCHANGED(Timeinhours__c) || ISCHANGED(StartTime__c) || ISCHANGED(Status__c ) )&amp;&amp;  
(PSA_Sync_Status__c == &apos;Success&apos; ||
	( PSA_Sync_Status__c ==&apos;Failed&apos; &amp;&amp; PSA_Sync_Count__c &gt;= $Setup.CSM_case_Trigger_Handler__c.SyncNumberOfAttempts__c))</errorConditionFormula>
        <errorMessage>Record is already synced with PSA and time can not be changed.</errorMessage>
    </validationRules>
