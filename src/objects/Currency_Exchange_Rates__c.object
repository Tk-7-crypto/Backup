    <fields>
        <fullName>Conversion_rate__c</fullName>
        <description>CPQ-IQ-LC-13849: Exchange rate value based on USD as base currency.</description>
        <externalId>false</externalId>
        <label>Conversion Rate</label>
        <precision>18</precision>
        <required>true</required>
        <scale>5</scale>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Effective_Date__c</fullName>
        <description>CPQ-IQ-LC-13849: Date for which the dated currency exchange rate actually applies, even if it is being used for a broader date range. Currency conversions would always look for most recent rate that is not in the future (i.e. today or earlier).</description>
        <externalId>false</externalId>
        <inlineHelpText>Date for which the dated currency exchange rate actually applies, even if it is being used for a broader date range. Currency conversions would always look for most recent rate that is not in the future (i.e. today or earlier).</inlineHelpText>
        <label>Effective Date</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Expiration_Date__c</fullName>
        <description>CPQ-IQ-LC-13849: Specifies the date on which this Exchange Rate expires, if any. The Exchange Rate is valid until the day before the Expiration Date, or never expires if blank.</description>
        <externalId>false</externalId>
        <inlineHelpText>Specifies the date on which this Exchange Rate expires, if any. The Exchange Rate is valid until the day before the Expiration Date, or never expires if blank.</inlineHelpText>
        <label>Expiration Date</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
