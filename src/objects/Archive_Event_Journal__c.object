    <validationRules>
        <fullName>AEJ_PSA_VR01_Cannot_Edit_MJC_Fields</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
OR(
PRIORVALUE( Name ) != Name
))</errorConditionFormula>
        <errorMessage>You Can Not Update Unique Id</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AEJ_PSA_VR03_DateLastJournalSearch</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND( Date_Last_Journal_Search_was_Performed__c &gt; TODAY(), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False)</errorConditionFormula>
        <errorMessage>Date Last Journal Search was Performed&apos; cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AEJ_PSA_VR04_inactiveSinceFuture</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND($Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False,AND(  ISPICKVAL( Active__c , &apos;No&apos;),Inactive_since__c &gt; TODAY()),!ISNEW() )</errorConditionFormula>
        <errorDisplayField>Inactive_since__c</errorDisplayField>
        <errorMessage>Inactive since? cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AEJ_PSA_VR05_NoInActive</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND( $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False,ISPICKVAL( Active__c ,&quot;No&quot;),ISNULL( Inactive_since__c),!ISNEW())</errorConditionFormula>
        <errorDisplayField>Inactive_since__c</errorDisplayField>
        <errorMessage>Inactive Since? cannot be empty, as No has been selected from the &apos;Active?&apos; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AEJ_PSA_VR06_validationOnOther</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND( ISPICKVAL( Frequency_of_Journal__c , &apos;Other&apos;) , ISBLANK( Other_Journal_Frequency_Specify__c ), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False, !ISNEW())</errorConditionFormula>
        <errorDisplayField>Other_Journal_Frequency_Specify__c</errorDisplayField>
        <errorMessage>&apos;Other Journal Frequency (Specify)&apos; cannot be empty, as &apos;Other&apos; has been selected from the &apos;Frequency of Journal&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AEJ_PSA_VR07_yesInActive</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND( $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False,AND(ISPICKVAL( Active__c ,&apos;Yes&apos;),!ISBLANK( Inactive_since__c )),!ISNEW() )</errorConditionFormula>
        <errorDisplayField>Inactive_since__c</errorDisplayField>
        <errorMessage>You have selected &apos;Yes&apos; in the &apos;Active?&apos; field, you cannot enter a value in the &apos;Inactive Since&apos; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AEJ_PSA_VR08_CheckDataFromLR</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == False, ISNEW(), 
OR(
ISBLANK(Account__c),
ISBLANK(Product__c),
ISBLANK(Project__c),
ISPICKVAL(LR_Type__c, &apos;&apos;)
)
)</errorConditionFormula>
        <errorMessage>You cannot create a new Event Journal record because Project, Account, Product and LR Type is blank on the LR project overview record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AEJ_PSA_VR09_Cannot_Blank_MJC_Fields</fullName>
        <active>true</active>
        <description>EJ_PSA_VR09_Cannot_Blank_MJC_Fields</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
OR(
ISBLANK( URL__c ),
ISBLANK( Therapeutic_Area__c ),
ISBLANK( Subscription__c ),
ISBLANK( Regulatory_Requirement__c ),
ISBLANK( Periodicity__c ),
ISBLANK( Paper_Journal__c ),
ISPICKVAL(Country__c , &apos;&apos;),
ISBLANK( Journal_Name__c ),
ISBLANK( Name )
)
)</errorConditionFormula>
        <errorMessage>You cannot Blank &quot;Unique ID, Country, Journal Name, URL, Periodicity, Paper Journal, Regulatory requirement, Subscription, Therapeutic Area &quot; fields.</errorMessage>
    </validationRules>
