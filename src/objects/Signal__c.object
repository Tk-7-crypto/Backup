)</errorConditionFormula>
        <errorMessage>You cannot create a new Signal Record because the project is Completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SIG_PSA_ProjectCompletedEditCase</fullName>
        <active>true</active>
        <description>IQVIAPSA-3327</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
NOT(ISNEW()),
ISPICKVAL(Project__r.pse__Stage__c,&apos;Completed&apos;)
)</errorConditionFormula>
        <errorMessage>You cannot edit Signal Record because the project is Completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SIG_PSA_VR01_SignalStatus</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(ISPICKVAL(Signal_Status__c,&quot;Not Validated&quot;), ISPICKVAL(Signal_Status__c,&quot;Closed&quot;)),ISNULL(Validation_Date__c), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False)</errorConditionFormula>
        <errorMessage>The ‘Validation Date&apos; cannot be empty if the ‘Signal Status&apos; is ‘Not Validated’ or ‘Closed’</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SIG_PSA_VR02_DateClosed</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Signal_Status__c ,&quot;Closed&quot;),ISNULL( Date_Closed_Evaluation_Completed__c ), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False)</errorConditionFormula>
        <errorMessage>The ‘Date Closed (Evaluation Completed)’ cannot be empty if the ‘Signal Status’ is ‘Closed’</errorMessage>
