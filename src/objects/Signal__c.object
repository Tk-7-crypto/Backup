        <fullName>SIG_PSA_ProjectCompleted</fullName>
        <active>true</active>
        <description>IQVIPSA-3327</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
ISNEW(),
