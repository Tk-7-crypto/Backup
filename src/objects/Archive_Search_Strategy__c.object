    <validationRules>
        <fullName>ASS_01_SearchStrategygotClientApproval</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND(Date_Search_Strategy_got_Client_Approval__c &gt; TODAY(), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == False)</errorConditionFormula>
        <errorMessage>&apos;Date Search Strategy got Client Approval&apos; cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASS_02_DateAnnualValidation</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND( ISNULL( Date_Annual_Search_Strategy_Due__c ), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Date_Annual_Search_Strategy_Due__c</errorDisplayField>
        <errorMessage>Date Annual Search Strategy Due&apos; field is a required field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASS_03_CheckDataFromLR</fullName>
        <active>true</active>
        <description>IQVIAPSA-4655</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == False, ISNEW(),
OR(
ISBLANK(Account__c),
ISBLANK(Product__c),
ISBLANK(Project__c),
ISPICKVAL(LR_Type__c, &apos;&apos;)
)
)</errorConditionFormula>
        <errorMessage>You cannot create a new search strategy record because Project, Account, Product and LR Type is blank on the LR project overview record.</errorMessage>
    </validationRules>
