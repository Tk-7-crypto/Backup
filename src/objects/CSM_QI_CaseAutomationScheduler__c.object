    <validationRules>
        <fullName>CAS_VR01_AssetIsRequired</fullName>
        <active>true</active>
        <description>CSM-1039 automation scheduler for Optrack</description>
        <errorConditionFormula>AND(Active__c, ISBLANK(Asset_Name__c))</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Asset can&apos;t be null when Active is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR02_CategorizationIsRequired</fullName>
        <active>true</active>
        <description>CSM-1039 automation scheduler for Optrack</description>
        <errorConditionFormula>AND(Active__c, ISBLANK(Case_Categorization__c))</errorConditionFormula>
        <errorMessage>Categorization should be filled in Categorization component on detail page before activating record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR03_WeekdayIsRequired</fullName>
        <active>true</active>
        <description>CSM-1039 automation scheduler for Optrack</description>
        <errorConditionFormula>AND(Active__c, 
				ISBLANK(FrequencyId__c), 
				ISPICKVAL(Frequency__c, &apos;Weekly&apos;))</errorConditionFormula>
        <errorMessage>Weekday should be selected in Frequency component on detail page before activating record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR04_MonthIntervalIsRequired</fullName>
        <active>true</active>
        <description>CSM-1039 automation scheduler for Optrack</description>
        <errorConditionFormula>AND(Active__c,
				ISBLANK(FrequencyId__c),
				OR(ISPICKVAL(Frequency__c, &apos;Monthly&apos;),
						ISPICKVAL(Frequency__c, &apos;Quarterly&apos;)))</errorConditionFormula>
        <errorMessage>Month date/week should be selected in Frequency component on detail page before activating record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR05_CurrentQueuesAreMandatory</fullName>
        <active>true</active>
        <description>CSM-1039 automation scheduler for Optrack</description>
        <errorConditionFormula>AND(Active__c,ISBLANK(Primary_Owner_Current_Queue__c),ISBLANK(Secondary_Owner_Current_Queue__c))</errorConditionFormula>
        <errorMessage>Primary/Secondary Owner Current Queue can&apos;t be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR06_AssetAccountCheck</fullName>
        <active>true</active>
        <description>CSM-2251:TECH - Schedule Case Automation - Asset update</description>
        <errorConditionFormula>AND(OR(Active__c, ISCHANGED( Account_Name__c)),Account_Name__c  &lt;&gt;  Asset_Name__r.AccountId)</errorConditionFormula>
        <errorDisplayField>Asset_Name__c</errorDisplayField>
        <errorMessage>Asset Account should be same as selected Account Name</errorMessage>
    </validationRules>
