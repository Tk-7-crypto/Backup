    <validationRules>
        <fullName>CFI_01_Other_AR_Type_Validation</fullName>
        <active>true</active>
        <description>IQVIAPSA-4651</description>
        <errorConditionFormula>AND( ISPICKVAL( AR_Type__c , &quot;Other&quot;) , ISBLANK( Other_AR_Type__c ), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c  == False )</errorConditionFormula>
        <errorDisplayField>Other_AR_Type__c</errorDisplayField>
        <errorMessage>&apos;Other AR Type&apos; cannot be empty, as &apos;Other&apos; has been selected from the &apos;AR Type&apos; field&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CFI_02_Future_Date_in_Date_Held</fullName>
        <active>true</active>
        <description>IQVIAPSA-4651</description>
        <errorConditionFormula>AND(Date_Held__c &gt; TODAY(),
 $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c== FALSE)</errorConditionFormula>
        <errorDisplayField>Date_Held__c</errorDisplayField>
        <errorMessage>&apos;Date Held&apos; cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CFI_03_Validation_on_LR_Represented</fullName>
        <active>true</active>
        <description>IQVIAPSA-4651</description>
        <errorConditionFormula>AND( ISPICKVAL( LR_Represented_CFI_Attendance__c , &quot;No&quot;) , ISPICKVAL( Rationale_of_LR_Non_Attendance__c , &quot;&quot;), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c ==False )</errorConditionFormula>
        <errorDisplayField>Rationale_of_LR_Non_Attendance__c</errorDisplayField>
        <errorMessage>&apos;Rationale of LR Non-Attendance&apos; cannot be empty if  &apos;No&apos; is selected from the &apos;LR Represented / CFI Attendance&apos; field&apos;</errorMessage> 
    </validationRules>
    <validationRules>
        <fullName>CFI_04_ValidationOnAggregateReports</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(LR_Project_Overview__r.Aggregate_Reports__c,&apos;No&apos;),  ISNEW() , $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c = False)</errorConditionFormula>
        <errorMessage>Aggregate Reports field is set to &apos;No&apos; you cannot create a CFI Meeting record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CFI_05_ValidationOnDateHeld</fullName>
        <active>true</active>
        <description>Date Held cannot be empty if &apos;Yes&apos; is selected from &apos;LR Represented/CFI Attendance&apos; field</description>
        <errorConditionFormula>AND( ISPICKVAL( LR_Represented_CFI_Attendance__c , &quot;Yes&quot;), ISNULL( Date_Held__c ), $Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c ==False )</errorConditionFormula>
        <errorMessage>Date Held cannot be empty as &apos;Yes&apos; has been selected from the &apos;LR Represented/CFI Attendance&apos; field</errorMessage>
    </validationRules>
