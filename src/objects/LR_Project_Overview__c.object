        <fullName>LR_VR03_On_LR_Type_Stop_Edit</fullName>
        <active>true</active>
        <description>IQVIAPSA-4585</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL( LR_Type__c , &quot;EV MLM&quot;),
ISPICKVAL( LR_Type__c , &quot;Global&quot;),
ISPICKVAL( LR_Type__c , &quot;SLR&quot;),
ISPICKVAL( LR_Type__c , &quot;Other&quot;)
),
OR(
 NOT( ISBLANK(Countries_Searched__c) ),
 NOT( ISBLANK(Journals_Screened_Reviewed__c) ),
AND(ISNEW(), OR(NOT(ISBLANK(Countries_Searched__c)), NOT(ISBLANK(Journals_Screened_Reviewed__c))))
),
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE
)</errorConditionFormula>
        <errorMessage>You cannot enter value in #Countries Searched&apos; and &apos;#Journals Screened / Reviewed&apos; fields if LR Type is selected &apos;EV MLM&apos;, &apos;Global&apos;, &apos;SLR&apos; or &apos;Other&apos;</errorMessage>
