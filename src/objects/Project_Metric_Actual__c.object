    <validationRules>
        <fullName>PRM_VR02_PSA_MetricRequired</fullName>
        <active>true</active>
        <description>PSA-USNONE-VR-PSAMetricRequired</description>
        <errorConditionFormula>ISBLANK(  Project_Metric__r.PSA_Metric__c )</errorConditionFormula>
        <errorMessage>PSA Metric cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_VR03_PSA_ProjectMetricRequired</fullName>
        <active>true</active>
        <description>PSA-USNONE-VR-ProjectMetricRequired</description>
        <errorConditionFormula>ISBLANK(Project_Metric__c)</errorConditionFormula>
        <errorDisplayField>Project_Metric__c</errorDisplayField>
        <errorMessage>Project Metric  cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_VR04_PSA_ProjectMismatch</fullName>
        <active>true</active>
        <description>PSA-USNONE-VR- The Metric’s Project must equal the Project lookup</description>
        <errorConditionFormula>Project_Metric__r.Project__c != Project__c</errorConditionFormula>
        <errorMessage>The Metric’s Project must equal the Project lookup</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_VR04_PSA_ProjectRequired</fullName>
        <active>true</active>
        <description>PSA-USNONE-VR-Project cannot be blank</description>
        <errorConditionFormula>ISBLANK(Project__c)</errorConditionFormula>
        <errorDisplayField>Project__c</errorDisplayField>
        <errorMessage>Project cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_VR05_PSA_PSAMetricMismatch</fullName>
        <active>true</active>
        <description>PSA-USNONE-VR- The Project Metric’s Metric Lookup must equal the Metric loookup</description>
        <errorConditionFormula>Project_Metric__r.PSA_Metric__c  !=  PSA_Metric__c</errorConditionFormula>
        <errorMessage>The Project Metric’s PSA Metric Lookup must equal the PSA Metric loookup</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_VR06_ProjectEndDateInPast</fullName>
        <active>false</active>
        <description>IQVIAPSA-3990</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
Project__r.pse__End_Date__c &lt; TODAY()
)</errorConditionFormula>
        <errorMessage>An error occurred while trying to update the record. The Project &apos;End Date&apos; is in the past. Please update the project &apos;End Date&apos; and try again</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_VR07_ProtocolStopDateInPast</fullName>
        <active>true</active>
        <description>IQVIAPSA-3990</description>
        <errorConditionFormula>AND(
$Setup.Mulesoft_Integration_Control__c.Ignore_Validation_Rules__c == FALSE,
 Project_Metric__r.Protocol_ID__r.Target__r.Stop_Date__c &lt; TODAY()
)</errorConditionFormula>
        <errorMessage>An error occurred while trying to update the record. The Protocol &apos;Stop Date&apos; is in the past. Please update the RDS Protocol ‘Stop Date’ and try again.</errorMessage>
    </validationRules>
