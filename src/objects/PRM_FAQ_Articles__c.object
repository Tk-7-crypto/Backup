<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>To add articles to be displayed on portal FAQ page.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Article_Link__c</fullName>
        <description>Add Article Link</description>
        <externalId>false</externalId>
        <label>Article Link</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IsActive__c</fullName>
        <defaultValue>true</defaultValue>
        <description>To check if article is active</description>
        <externalId>false</externalId>
        <label>IsActive</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Order__c</fullName>
        <description>To specify order of articles</description>
        <externalId>false</externalId>
        <label>Order</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>PRM FAQ Articles</label>
    <visibility>Public</visibility>
</CustomObject>
