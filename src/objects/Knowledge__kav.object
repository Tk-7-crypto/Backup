    <validationRules>
        <fullName>KNO_VR01_CSM_CreatorCannotBeReviewer</fullName>
        <active>true</active>
        <description>CSM-ACN-S-0163</description>
        <errorConditionFormula>NOT($Permission.CSM_Bypass_CSM_Validation_Rules)
&amp;&amp; Test__c = $User.Id</errorConditionFormula>
        <errorMessage>You cannot assign yourself as reviewer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>KNO_VR02_CSM_CheckReviewerPS</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($Permission.CSM_Bypass_CSM_Validation_Rules)
&amp;&amp; Test__r.Article_Manager__c = False</errorConditionFormula>
        <errorMessage>The Reviewer Selected is not a KB Article Manager</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>KNO_VR03_PRM_VisibleToPartner</fullName>
        <active>true</active>
        <description>If visible to partner is checked then select Partner Type</description>
        <errorConditionFormula>AND(IsVisibleInPrm, ISBLANK(Partner_Type__c))</errorConditionFormula>
        <errorMessage>Select Partner Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>KNO_VR04_CSM_RD_ValidationStatusCheck</fullName>
        <active>true</active>
        <description>CSM-1870: R&amp;D - Validation Status should be Validated only if PublishStatus is Published for R&amp;D by Approval Process</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISPICKVAL(Pillar__c, &apos;R&amp;D&apos;),  ISPICKVAL(PublishStatus, &apos;Draft&apos;), NOT(ISCHANGED(PublishStatus)),NOT($Permission.CSM_Bypass_CSM_Validation_Rules),
ISCHANGED(ValidationStatus), ISPICKVAL(ValidationStatus, &apos;Validated&apos;))</errorConditionFormula>
        <errorMessage>Validation Status should not be changed to Validated</errorMessage>
    </validationRules>
</CustomObject>
