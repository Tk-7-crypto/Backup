    <description>CSM-1950 Auto Close Parent Sttus Bach and CSM-1321 Case Escalated Filed should be Checked After the Status Closed</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(PRIORVALUE([Case].IsEscalated) , ISCHANGED([Case].IsClosed ) , [Case].IsClosed,
NOT(ISPICKVAL([Case].Status, &apos;Merged&apos;)) )</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(PRIORVALUE({!myVariable_current.IsEscalated}) , ISCHANGED({!myVariable_current.IsClosed} ) , {!myVariable_current.IsClosed},
NOT(ISPICKVAL({!myVariable_current.Status}, &apos;Merged&apos;)) )</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(ISCHANGED([Case].Status), ISCHANGED([Case].Owner__c), ISPICKVAL([Case].Status , &apos;In Progress&apos;), LEFT([Case].OwnerId , 3) &lt;&gt; &apos;00G&apos;, [Case].Owner__c =&apos;Child Open&apos;)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
