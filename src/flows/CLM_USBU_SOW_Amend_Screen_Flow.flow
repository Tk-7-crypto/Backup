    <formulas>
        <name>forT_AccountLink</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_140},FIND(&apos;.com&apos;,{!$Api.Partner_Server_URL_140})+4)+{!varR_agreement.Account__c}</expression>
    </formulas>
    <formulas>
        <name>forT_amendAGrName</name>
        <dataType>String</dataType>
        <expression>IF(LEN({!type} +&apos;_&apos; +TEXT({!Get_Version_Number.Version_Number__c} + 1) + &apos;_&apos; + IF({!varR_agreement.Version_Number__c} == 0, {!varR_agreement.Name}, {!varR_agreement.Original_Agreement__r.Name})) &gt; 80
, LEFT({!type}+&apos;_&apos; + TEXT({!Get_Version_Number.Version_Number__c} + 1) + &apos;_&apos; + IF({!varR_agreement.Version_Number__c} == 0, {!varR_agreement.Name}, {!varR_agreement.Original_Agreement__r.Name}), 80)
, {!type} +&apos;_&apos;+ TEXT({!Get_Version_Number.Version_Number__c} + 1) + &apos;_&apos; + IF({!varR_agreement.Version_Number__c} == 0, {!varR_agreement.Name}, {!varR_agreement.Original_Agreement__r.Name}))</expression>
    </formulas>
    <formulas>
