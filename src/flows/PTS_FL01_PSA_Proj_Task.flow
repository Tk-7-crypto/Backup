    <description>This flow is used to trigger email notifications for Project Task object</description>
    <environments>Default</environments>
    <formulas>
        <name>enddateandcreateddatedifference</name>
        <dataType>Number</dataType>
        <expression>DATEVALUE({!$Record.pse__End_Date_Time__c}) - DATEVALUE({!$Record.CreatedDate})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>PTS_WEA02_PSA_CLD_PSAProjectTaskPastDueAlert_Formula</name>
        <dataType>Boolean</dataType>
        <expression>OR({!$Record.Trigger_Past_Due_Workflow__c}&amp;&amp; TEXT({!$Record.pse__Status__c}) != &apos;Complete&apos; &amp;&amp; {!$Record.pse__End_Date__c} &lt;= Today() &amp;&amp; !ISBLANK({!$Record.Resource__c}) &amp;&amp; OR({!$Record.RecordType.DeveloperName} = &apos;RDS_Deliverable_Task&apos;,{!$Record.RecordType.DeveloperName}= &apos;RDS_Project_Task&apos;), {!$Record.Trigger_Past_Due_Workflow__c}&amp;&amp; TEXT({!$Record.pse__Status__c}) != &apos;Complete&apos; &amp;&amp; {!$Record.End_Date_Planned__c}&lt;= Today() &amp;&amp; !ISBLANK({!$Record.Resource__c}) &amp;&amp; {!$Record.RecordType.DeveloperName} = &apos;RDS_Deliverable&apos; )</expression>
    </formulas>
    <formulas>
        <name>PTS_WR01_PSA_DateUpcomingNotification_Formula</name>
        <dataType>Boolean</dataType>
        <expression>AND ( OR({!$Record.RecordType.DeveloperName} = &apos;RDS_Deliverable&apos;, {!$Record.RecordType.DeveloperName} =&apos;RDS_Deliverable_Task&apos;), {!$Record.Service_Line_Code__c} != &apos;PVA&apos;, OR(TEXT({!$Record.pse__Status__c}) == &apos;In Progress&apos;, TEXT({!$Record.pse__Status__c}) == &apos;Planned&apos;) , ISBLANK( {!$Record.pse__Actual_End_Date_Time__c}), NOT(ISBLANK({!$Record.Resource__c})), VALUE(TEXT({!$Record.pse__End_Date__c}- DATEVALUE({!$Record.CreatedDate}))) &gt; 7 )</expression>
    </formulas>
    <formulas>
        <name>PTS_WR02_PSA_NewTaskNotification_Formula</name>
        <dataType>Boolean</dataType>
        <expression>AND ( OR ( {!$Record.RecordType.DeveloperName}= &apos;RDS_Deliverable&apos;, {!$Record.RecordType.DeveloperName} = &apos;RDS_Deliverable_Task&apos; ), TEXT({!$Record.pse__Status__c}) != &apos;Complete&apos;, OR ( (ISNEW() &amp;&amp; !ISBLANK({!$Record.Resource__c})), (ISCHANGED({!$Record.Resource__c}) &amp;&amp; !ISBLANK({!$Record.Resource__c})) ) )</expression>
    </formulas>
    <formulas>
        <description>IQVIAPSA-3865, IQVIAPSA-4697, IQVIAPSA-5640</description>
        <name>PTS_WR07_PSA_Date_SrvcLineLead_UpcomingNotification_Mail</name>
        <dataType>Boolean</dataType>
        <expression>AND( AND({!$Record.RecordType.DeveloperName} = &apos;RDS_Project_Task&apos;,  {!$Record.Trigger_Email_Notification__c} = True), {!$Record.Service_Line_Lead__c} != null, IF( ISBLANK({!$Record.pse__Actual_End_Date__c}), TRUE, FALSE), IF({!$Record.pse__End_Date__c}  &gt;  TODAY(), TRUE, FALSE),  NOT( AND({!$Record.Service_Line__r.Name} = &apos;PV Solutions&apos;, CONTAINS({!$Record.Name}, &apos;Complete Project Files Review&apos;))), {!$Record.Activate_Workflow_Rule_Conditionally__c} = TRUE)</expression>
    </formulas>
