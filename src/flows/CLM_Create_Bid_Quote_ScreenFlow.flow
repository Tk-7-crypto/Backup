    <description>CLM-MC-CLMCMA-16597 - This flow is used to create Quote from BidHistory.</description>
    <environments>Default</environments>
    <formulas>
        <name>forT_DefaultName</name>
        <dataType>String</dataType>
        <expression>{!Get_BidHistory.Bid_History_Related_Opportunity__r.Legacy_Quintiles_Opportunity_Number__c}+&apos;_CNF Quote&apos;</expression>
    </formulas>
    <formulas>
        <name>forT_ExistingQuoteLink</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260})) + {!Get_Existing_Quote.Id}</expression>
    </formulas>
    <formulas>
        <name>forT_FinalDivision</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!Get_BidHistory.Division_Business_Unit__c}) == &apos;Clinical&apos;, &apos;Core Clinical&apos;, 
IF(TEXT({!Get_BidHistory.Division_Business_Unit__c}) == &apos;Data Sciences&apos;, &apos;Data Management&apos;, 
IF(TEXT({!Get_BidHistory.Division_Business_Unit__c}) == &apos;Q2 Solutions&apos;, &apos;Lab&apos;, 
IF(TEXT({!Get_BidHistory.Division_Business_Unit__c}) == &apos;Safety &amp; Med Info&apos;, &apos;Lifecycle Safety&apos;, TEXT({!Get_BidHistory.Division_Business_Unit__c})))))</expression>
    </formulas>
    <formulas>
        <name>forT_QuoteType</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!Get_BidHistory.Requested_Agreement_Type__c}) == &quot;CNF&quot;, &quot;CNF Quote&quot;, TEXT({!Get_BidHistory.Requested_Agreement_Type__c}))</expression>
    </formulas>
