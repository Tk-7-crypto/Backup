    <decisions>
        <name>Check_Budget_Tools</name>
        <label>Check Budget Tools</label>
        <locationX>372</locationX>
        <locationY>562</locationY>
        <defaultConnector>
            <targetReference>Budget_Tools_Error_Screen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Invalid</defaultConnectorLabel>
        <rules>
            <name>Valid</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Get_BidHistory.Budget_Tools__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>No QIP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Budget_Tools__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>QIP Spreadsheet</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Budget_Tools__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>POP Spreadsheet</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Budget_Tools__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Outcome</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_RecordTypeId</targetReference>
            </connector>
            <label>Valid</label>
        </rules>
    </decisions>
