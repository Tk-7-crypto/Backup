    <formulas>
        <name>Agreement_End_Date</name>
        <dataType>Date</dataType>
        <expression>IF({!Agreement_Start_Date} &gt;= DATE(YEAR(TODAY()), 07, 01), DATE(YEAR(TODAY()) + 1, 12, 31), IF({!Agreement_Start_Date} &lt; DATE(YEAR(TODAY()), 07, 01), DATE(YEAR(TODAY()), 12, 31), NULL))</expression>
    </formulas>
    <formulas>
        <name>AMA_Address_RecordTypeId</name>
        <dataType>String</dataType>
        <expression>{!$Setup.CLM_Configuration__c.AMA_Address_RecordTypeId__c}</expression>
    </formulas>
    <formulas>
        <name>Concatenated_Address</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(
         IF( ISBLANK({!Address_Street}),NULL, {!Address_Street} + &apos;, &apos;)+
         IF( ISBLANK({!Address_City}),NULL, {!Address_City} + &apos;, &apos;)+
         IF( ISBLANK({!Address_State}),NULL, {!Address_State} + &apos;, &apos;)+
         IF( ISBLANK({!Address_Postal}),NULL, {!Address_Postal} + &apos;, &apos;)+
         IF( ISBLANK({!Address_Country}),NULL, {!Address_Country} + &apos;, &apos;)+&apos;.&apos;, &apos;, .&apos;, NULL)</expression>
    </formulas>
    <formulas>
        <name>ContactFormula</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!Combined_Contact}, &apos;, ,&apos;, &apos;, &apos;)</expression>
    </formulas>
    <formulas>
        <name>ContractCommaRemoves</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(
        IF( ISBLANK({!Contact_First_Name}),NULL, {!Contact_First_Name} + &apos;  &apos;)+
        IF( ISBLANK({!Contact_Last_Name}),NULL, {!Contact_Last_Name} + &apos;, &apos;)+
        IF( ISBLANK({!Contact_Email}),NULL, {!Contact_Email} + &apos;, &apos;)+
        IF( ISBLANK({!Contact_Number}),NULL, {!Contact_Number} + &apos;, &apos;)+&apos;.&apos;, &apos;, .&apos;, NULL)</expression>
    </formulas>
    <formulas>
        <name>Current_Year</name>
        <dataType>Number</dataType>
        <expression>YEAR(TODAY())</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>File_URL</name>
        <dataType>String</dataType>
        <expression>{!$Setup.CLM_Configuration__c.AMA_File_Link__c}</expression>
    </formulas>
    <formulas>
        <name>Final_Output_ActualCost</name>
        <dataType>Currency</dataType>
        <expression>{!Output_ActualCost} * ({!Proration_Type}/100)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Final_Output_RepCost</name>
        <dataType>Currency</dataType>
        <expression>{!Output_RepCost} * ({!Proration_Type}/100)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Output_Total_Agreement_Value</name>
        <dataType>Currency</dataType>
        <expression>(({!Output_ActualCost} + {!Output_RepCost}) * {!Proration_Type}) / 100</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Proration_Type</name>
        <dataType>Number</dataType>
        <expression>IF(NOT({!Is_Auto_Renew}), 100, 
IF(({!Agreement_Start_Date} &gt;= DATE(YEAR({!Agreement_Start_Date}), 1, 1) &amp;&amp; {!Agreement_Start_Date} &lt; DATE(YEAR({!Agreement_Start_Date}), 7, 1)), 100, 
IF(({!Agreement_Start_Date} &gt;= DATE(YEAR({!Agreement_Start_Date}), 7, 1) &amp;&amp; {!Agreement_Start_Date} &lt; DATE(YEAR({!Agreement_Start_Date}), 10, 1)), 50, 
IF(({!Agreement_Start_Date} &gt;= DATE(YEAR({!Agreement_Start_Date}), 10, 1) &amp;&amp; {!Agreement_Start_Date} &lt; DATE(YEAR({!Agreement_Start_Date}), 12, 15)), 25, 0
))))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>RecordCost_Custom_Calculated</name>
        <dataType>Currency</dataType>
        <expression>{!Per_Record_Cost} * {!Enter_the_number_of_records_requested}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Records_Quantity</name>
        <dataType>String</dataType>
        <expression>IF({!Final_Subcategory} == {!Contract_Salesforce_Subcategory}, &quot;&quot;,IF({!Final_Slider_Value} == {!Min_Record_Commitment}, &quot;Minimum&quot;, IF({!Final_Slider_Value} == {!Max_Record_Count}, &quot;Maximum&quot;,&quot;&quot;)))</expression>
    </formulas>
    <formulas>
        <name>RecordTypeId</name>
        <dataType>String</dataType>
        <expression>{!$Setup.CLM_Configuration__c.AMA_RecordTypeId__c}</expression>
    </formulas>
    <formulas>
        <name>Sales_Rep_Tier_Auto</name>
        <dataType>String</dataType>
        <expression>IF({!Number_of_Sales_Reps} &lt;= 10, &quot;0-10&quot;, IF({!Number_of_Sales_Reps} &lt;= 50, &quot;11-50&quot;, IF({!Number_of_Sales_Reps} &lt;= 100, &quot;51-100&quot;, IF({!Number_of_Sales_Reps} &lt;= 250, &quot;101-250&quot;, IF({!Number_of_Sales_Reps} &lt;= 500, &quot;251-500&quot;, IF({!Number_of_Sales_Reps} &lt;= 650, &quot;501-650&quot;, IF({!Number_of_Sales_Reps} &lt;= 950, &quot;651-950&quot;, IF({!Number_of_Sales_Reps} &lt;= 1400, &quot;951-1400&quot;, IF({!Number_of_Sales_Reps} &lt;= 1750, &quot;1401-1750&quot;, IF({!Number_of_Sales_Reps} &lt;= 2900, &quot;1751-2900&quot;, IF({!Number_of_Sales_Reps} &lt;= 3999, &quot;2901-3999&quot;, &quot;4000 and greater&quot;)))))))))))</expression>
    </formulas>
    <formulas>
        <name>Total_Agreement_Value_Before</name>
        <dataType>Currency</dataType>
        <expression>IF({!Final_Subcategory} == {!Contract_Salesforce_Subcategory}, 37000, {!Output_ActualCost} + {!Output_RepCost})</expression>
        <scale>2</scale>
    </formulas>
