    <decisions>
        <name>Check_Contract_Type</name>
        <label>Check Contract Type</label>
        <locationX>314</locationX>
        <locationY>1070</locationY>
        <defaultConnector>
            <targetReference>Create_Relationship_Record</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Customer Agreement Selected</defaultConnectorLabel>
        <rules>
            <name>MICA</name>
            <conditionLogic>1 OR 2</conditionLogic>
            <conditions>
                <leftValueReference>Get_Contract.Requested_Contract_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Master Independent Contract Agreement (MICA) Amendment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varR_SelectedCustomerAgr</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Navigate_to_Amended_Vendor_Agreement</targetReference>
            </connector>
            <label>MICA</label>
        </rules>
    </decisions>
