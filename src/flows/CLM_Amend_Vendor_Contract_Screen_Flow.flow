    <decisions>
        <name>Check_If_Original_Agreement_is_MSA_CO_or_Amendment</name>
        <label>Check If Original Agreement is MSA, CO or Amendment</label>
        <locationX>908</locationX>
        <locationY>398</locationY>
        <defaultConnector>
            <targetReference>Set_Related_Opportunity</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Otherwise</defaultConnectorLabel>
        <rules>
            <name>Yes_1</name>
            <conditionLogic>((1 OR 2 OR 3) AND 4) OR 5 OR 6 OR 7</conditionLogic>
            <conditions>
                <leftValueReference>Get_Vendor_Agreement.Apttus__Subtype__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Master Services Agreement</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Vendor_Agreement.Apttus__Subtype__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Amendment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Vendor_Agreement.Apttus__Subtype__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Change Order</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Vendor_Agreement.Apttus__Version_Number__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Vendor_Agreement.Original_Agreement__r.Apttus__Subtype__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Master Services Agreement</stringValue>
