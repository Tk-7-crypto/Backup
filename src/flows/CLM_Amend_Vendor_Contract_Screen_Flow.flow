If(OR(Text({!varR_SelectedRecord.Apttus__Subtype__c}) == &apos;Work Order&apos;, Text({!varR_SelectedRecord.Apttus__Subtype__c}) == &apos;Change Order&apos;,Text({!varR_SelectedRecord.Apttus__Subtype__c}) == &apos;Standalone Services Agreement&apos;) , &apos;Change Order&apos;,
If(Text({!varR_SelectedRecord.Apttus__Subtype__c}) == &apos;Termination Agreements&apos;, &apos;Termination Agreements&apos;, &apos;Amendment&apos;)))</expression>
