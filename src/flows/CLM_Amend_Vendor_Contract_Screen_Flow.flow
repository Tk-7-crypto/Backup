                <rightValue>
                    <stringValue>Amendment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Through_Activated_Agrs_For_Vendor_CO.Original_Agreement__c</leftValueReference>
