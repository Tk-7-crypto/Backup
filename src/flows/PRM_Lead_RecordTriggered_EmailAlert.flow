    <formulas>
        <name>LeadConversion</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.CreatedBy.Profile.Name} = &apos;PRM Community User&apos;&amp;&amp; {!$Record.RecordType.DeveloperName} = &apos;PRM_Lead&apos; &amp;&amp;                              ISCHANGED({!$Record.Status}) &amp;&amp;  ISPICKVAL({!$Record.Status} , &apos;Converted&apos;)</expression>
    </formulas>
    <formulas>
        <name>LeadConversionByAM</name>
        <dataType>Boolean</dataType>
        <expression>NOT({!$Record.CreatedBy.Profile.Name} = &apos;PRM Community User&apos;)  &amp;&amp; {!$Record.RecordType.DeveloperName} = &apos;PRM_Lead&apos; &amp;&amp; ISCHANGED({!$Record.Status}) &amp;&amp;  ISPICKVAL({!$Record.Status} , &apos;Converted&apos;) &amp;&amp; NOT ISBLANK( {!$Record.TECH_Alliance_Manager__c})</expression>
    </formulas>
    <formulas>
        <name>LeadCreationByAM</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.RecordType.DeveloperName} = &apos;PRM_Lead&apos; &amp;&amp;  ISPICKVAL( {!$Record.Status} ,&apos;Registered&apos;)&amp;&amp; ( ISCHANGED( {!$Record.TECH_Alliance_Manager__c}) || (ISNEW() &amp;&amp; NOT ISBLANK( {!$Record.TECH_Alliance_Manager__c})))</expression>
    </formulas>
    <formulas>
        <name>LeadRejection</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.CreatedBy.Profile.Name} = &apos;PRM Community User&apos;&amp;&amp; {!$Record.RecordType.DeveloperName} = &apos;PRM_Lead&apos; &amp;&amp;                                ISCHANGED({!$Record.Status}) &amp;&amp;  ISPICKVAL({!$Record.Status} , &apos;Closed-Rejected&apos;)</expression>
    </formulas>
    <formulas>
        <name>LeadRejectionByAM</name>
        <dataType>Boolean</dataType>
        <expression>NOT({!$Record.CreatedBy.Profile.Name} = &apos;PRM Community User&apos;) &amp;&amp; {!$Record.RecordType.DeveloperName} = &apos;PRM_Lead&apos; &amp;&amp;  ISCHANGED({!$Record.Status}) &amp;&amp;  ISPICKVAL({!$Record.Status} , &apos;Closed-Rejected&apos;) &amp;&amp; NOT ISBLANK( {!$Record.TECH_Alliance_Manager__c})</expression>
    </formulas>
