            &lt;td style=&quot;border-right: 1px solid&quot;&gt;Opportunity&lt;/td&gt;
            &lt;td style=&quot;padding-left:10px;&quot;&gt;
            &lt;a href=&quot;{!forT_OpportunityLink}&quot;&gt;{!Get_IQVIA_Agreement.Related_Opportunity__r.Opportunity_Number__c} - {!Get_IQVIA_Agreement.Related_Opportunity__r.Name}&lt;/a&gt;
