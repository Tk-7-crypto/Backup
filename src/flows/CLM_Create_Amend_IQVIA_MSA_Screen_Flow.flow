, LEFT(&apos;Amendment_&apos; + TEXT({!Get_Version_Number.Version_Number__c} + 1) + &apos;_&apos; + IF({!varR_AmendAgreement.Version_Number__c} == 0, {!varR_AmendAgreement.Name}, {!varR_AmendAgreement.Original_Agreement__r.Name}), 80)
, &apos;Amendment_&apos; + TEXT({!Get_Version_Number.Version_Number__c} + 1) + &apos;_&apos; + IF({!varR_AmendAgreement.Version_Number__c} == 0, {!varR_AmendAgreement.Name}, {!varR_AmendAgreement.Original_Agreement__r.Name}))</expression>
