    <formulas>
        <description>This extracts one CNF id from the String</description>
        <name>extractOneCNFId</name>
        <dataType>String</dataType>
        <expression>TRIM(LEFT({!SelectedCNFsOnPopup},FIND(&quot;;&quot;,{!SelectedCNFsOnPopup})-1))</expression>
    </formulas>
    <formulas>
        <description>It extracts CNF id from the id string</description>
        <name>extractOneCNFIdDelink</name>
        <dataType>String</dataType>
        <expression>TRIM(LEFT({!defaultSelectedCNFIds},FIND(&quot;;&quot;,{!defaultSelectedCNFIds})-1))</expression>
    </formulas>
    <formulas>
        <description>This will set value as 0 if its null</description>
        <name>formattedActualQuoteAmount</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK(TEXT({!Iterate_CNFs.Actual_Quote_Amount__c})), &apos;0&apos;, TEXT({!Iterate_CNFs.Actual_Quote_Amount__c}))</expression>
    </formulas>
    <formulas>
        <description>set value as BLANK if approval date is not set</description>
        <name>formattedApprovalDate</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK(TEXT({!Iterate_CNFs.CNF_Approval_Date__c})), &apos;BLANK&apos;, TEXT({!Iterate_CNFs.CNF_Approval_Date__c}))</expression>
    </formulas>
    <formulas>
        <description>This will substitute BLANK if the status is empty</description>
        <name>formattedCNFStatus</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK(TEXT({!Iterate_CNFs.CNF_Status__c})), &apos;BLANK&apos;, TEXT({!Iterate_CNFs.CNF_Status__c}))</expression>
    </formulas>
    <formulas>
        <description>Set value as BLANK if its null</description>
        <name>formattedCONumber</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK(TEXT({!Iterate_CNFs.Change_Order_Number__c})), &apos;BLANK&apos;, TEXT({!Iterate_CNFs.Change_Order_Number__c}))</expression>
    </formulas>
    <formulas>
        <description>Set value to BLANK if its null</description>
        <name>formattedCurrency</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK(TEXT({!Iterate_CNFs.CurrencyIsoCode})), &apos;BLANK&apos;, TEXT({!Iterate_CNFs.CurrencyIsoCode}))</expression>
    </formulas>
    <formulas>
        <description>Sets value BLANK if its null</description>
        <name>formattedName</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Iterate_CNFs.Name}), &apos;BLANK&apos;, {!Iterate_CNFs.Name})</expression>
    </formulas>
    <formulas>
        <description>Set the value to Blank if its null</description>
        <name>formattedScenario</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK(TEXT({!Iterate_CNFs.RFP_Scenario__c})), &apos;BLANK&apos;, TEXT({!Iterate_CNFs.RFP_Scenario__c}))</expression>
    </formulas>
    <formulas>
        <description>This removes the iterated CNF</description>
        <name>RemoveOneCNFId</name>
        <dataType>String</dataType>
        <expression>TRIM(SUBSTITUTE({!SelectedCNFsOnPopup},{!extractOneCNFId}+&quot;;&quot;,&quot;&quot;))</expression>
    </formulas>
    <formulas>
        <description>It removes the processed id from the string</description>
        <name>RemoveOneCNFIdDelink</name>
        <dataType>String</dataType>
        <expression>TRIM(SUBSTITUTE({!defaultSelectedCNFIds},{!extractOneCNFIdDelink}+&quot;;&quot;,&quot;&quot;))</expression>
    </formulas>
    <formulas>
        <description>Extract the Scenario Number to be assigned</description>
        <name>ScenarioNumber</name>
        <dataType>String</dataType>
        <expression>TRIM(SUBSTITUTE({!ChangeOrderNumber},&quot;Change Order &quot;,&quot;&quot;))</expression>
    </formulas>
