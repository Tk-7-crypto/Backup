    <recordLookups>
        <description>This &quot;Get Records&quot; action is used to get all the record details of the change order record on which the CNF linking popup is opened.</description>
        <name>fetchChangeOrderRecordDetails</name>
        <label>fetchChangeOrderRecordDetails</label>
        <locationX>424</locationX>
        <locationY>158</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>getEligibleCNFRecords</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Apttus_Proposal__Proposal__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <description>This &quot;Get Record&quot; action will fetch all the CNF records that are eligible for linking.</description>
        <name>getEligibleCNFRecords</name>
        <label>getEligibleCNFRecords</label>
        <locationX>424</locationX>
        <locationY>278</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>eligibleCNFsExists</targetReference>
        </connector>
        <filterLogic>1 AND 2 AND 3 AND (4 OR 5)</filterLogic>
        <filters>
            <field>Apttus_Proposal__Proposal_Category__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>CNF</stringValue>
            </value>
        </filters>
        <filters>
            <field>Apttus_Proposal__Opportunity__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>fetchChangeOrderRecordDetails.Apttus_Proposal__Opportunity__c</elementReference>
            </value>
        </filters>
        <filters>
            <field>CNF_Status__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Rejected</stringValue>
            </value>
        </filters>
        <filters>
            <field>Related_Change_Order__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <filters>
            <field>Related_Change_Order__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>Apttus_Proposal__Proposal__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
