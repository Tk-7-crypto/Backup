    <decisions>
        <name>Update_When_CO_Is_Initiated_On_WO</name>
        <label>Update When CO Is Initiated On WO</label>
        <locationX>407</locationX>
        <locationY>158</locationY>
        <defaultConnectorLabel>CO not initiated</defaultConnectorLabel>
        <rules>
            <name>CO_Is_Initiated</name>
            <conditionLogic>(1 AND 2) AND (3 OR 4 OR 7) AND (5 OR 6)</conditionLogic>
            <conditions>
                <leftValueReference>varR_NewRelatedAgreement.Apttus__Relationship_From_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Is Amended By</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varR_NewRelatedAgreement.Apttus__Relationship_To_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Is Amendment For</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varR_NewRelatedAgreement.Apttus__APTS_Contract_From__r.Record_Type_Developer_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>PSA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varR_NewRelatedAgreement.Apttus__APTS_Contract_From__r.Record_Type_Developer_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Preliminary_Agreement</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varR_NewRelatedAgreement.Apttus__APTS_Contract_To__r.Apttus__Subtype__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Change Order</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varR_NewRelatedAgreement.Apttus__APTS_Contract_To__r.Apttus__Subtype__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Amendment</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varR_NewRelatedAgreement.Apttus__APTS_Contract_From__r.Record_Type_Developer_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RWLP</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Clinical_Agreement</targetReference>
            </connector>
            <label>CO Is Initiated</label>
        </rules>
    </decisions>
