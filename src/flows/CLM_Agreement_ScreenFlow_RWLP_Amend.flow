    <decisions>
        <name>Bid_Subtype_Selection</name>
        <label>Bid Subtype Selection?</label>
        <locationX>1363</locationX>
        <locationY>1375</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Late_Phase_PS_1</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>Get_BidHistory.Agreement_Pathway__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Late Phase</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Agreement_Pathway__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RBU</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Requested_Agreement_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New Change Order</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Activated_Agreements_From_Opportunity_LPPS</targetReference>
            </connector>
            <label>Late Phase PS</label>
        </rules>
        <rules>
            <name>Late_Phase_Prelim_1</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>Get_BidHistory.Agreement_Pathway__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Late Phase</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Agreement_Pathway__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RBU</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Requested_Agreement_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Preliminary Agreement Amendment</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Activated_Agreements_From_Opportunity_LP_Prelim</targetReference>
            </connector>
            <label>Late Phase Prelim</label>
        </rules>
        <rules>
            <name>Clinical_Prelim_1</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_BidHistory.Agreement_Pathway__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Clinical</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Requested_Agreement_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Preliminary Agreement Amendment</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Activated_Agreements_From_Opportunity_Clinical_Prelim</targetReference>
            </connector>
            <label>Clinical Prelim</label>
        </rules>
        <rules>
            <name>OSA_Amendment_1</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_BidHistory.Agreement_Pathway__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Other Special Agreement</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BidHistory.Requested_Agreement_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>LRA Amendment</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Activated_Agreements_From_Opportunity_OSA</targetReference>
            </connector>
            <label>OSA Amendment</label>
        </rules>
    </decisions>
