    <formulas>
        <name>forT_BidHistoryRequestedAgreementType</name>
        <dataType>String</dataType>
        <expression>IF(OR(({!Get_Agreement.Record_Type_Developer_Name__c}  == &apos;Preliminary_Agreement&apos;), 
AND({!Get_Agreement.Record_Type_Developer_Name__c} == &apos;RWLP&apos;,
OR(ISPICKVAL({!Get_Agreement.Apttus__Subtype__c},&apos;ATP&apos;), ISPICKVAL({!Get_Agreement.Apttus__Subtype__c}, &apos;SUWO&apos;), ISPICKVAL({!Get_Agreement.Apttus__Subtype__c}, &apos;Amendment&apos;)))), &apos;Preliminary Agreement Amendment&apos;,
IF(OR(({!Get_Agreement.Record_Type_Developer_Name__c}  == &apos;PSA&apos;), 
AND({!Get_Agreement.Record_Type_Developer_Name__c} == &apos;RWLP&apos;, 
NOT(ISPICKVAL({!Get_Agreement.Apttus__Subtype__c}, &apos;ATP&apos;)), NOT(ISPICKVAL({!Get_Agreement.Apttus__Subtype__c} , &apos;SUWO&apos;)), NOT(ISPICKVAL({!Get_Agreement.Apttus__Subtype__c}, &apos;Amendment&apos;)))), &apos;New Change Order&apos;, 
IF({!Get_Agreement.Record_Type_Developer_Name__c} == &apos;OSA&apos;, &apos;LRA Amendment&apos;, &apos;&apos;)))</expression>
    </formulas>
    <formulas>
        <name>forT_BidPathway</name>
        <dataType>String</dataType>
        <expression>IF(OR({!Get_Agreement.Record_Type_Developer_Name__c} == &apos;Preliminary_Agreement&apos;,
{!Get_Agreement.Record_Type_Developer_Name__c} == &apos;PSA&apos;), &apos;Clinical&apos;, 
IF({!Get_Agreement.Record_Type_Developer_Name__c} == &apos;RWLP&apos;, &apos;Late Phase&apos;, 
IF({!Get_Agreement.Record_Type_Developer_Name__c} == &apos;OSA&apos;, &apos;Other Special Agreement&apos;, &apos;&apos;)))</expression>
    </formulas>
