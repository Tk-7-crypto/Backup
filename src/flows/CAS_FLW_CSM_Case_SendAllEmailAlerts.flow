OR({!$Record.CreatedById} = &apos;0056A000000jAPAQA2&apos;,{!$Record.CreatedById} = &apos;0056A000000jAPA&apos;)), /* Auto case System User */ 
OR({!$Record.RecordTypeName__c} = &apos;TechnologyCase&apos;, 
{!$Record.RecordTypeName__c} = &apos;DATACase&apos;),
OR ({!$Record.SuppliedEmail} &lt;&gt; &apos;cz-hotline@cz.imshealth.com&apos;,
{!$Record.SuppliedEmail} &lt;&gt; &apos;nl.onekey-updates@iqvia.com&apos;,
{!$Record.SuppliedEmail} &lt;&gt; &apos;cstrain01@us.imshealth.com&apos;),
OR({!$Record.From_EmailToCase__c} = &apos;replytocsm@iqvia.com&apos;, 
{!$Record.From_EmailToCase__c} = &apos;email2casecsm@iqvia.com&apos;,
{!$Record.From_EmailToCase__c} = &apos;email2casecsm@quintilesims.com&apos;))</expression>
    </formulas>
    <formulas>
        <name>AllCasePortalDefaultCondition</name>
        <dataType>Boolean</dataType>
        <expression>AND( ISNEW(),
ISBLANK({!$Record.ParentId}), 
{!AllLastModifiedMinusCreatedDate}, 
OR({!$Profile.Name} = &apos;CSM Customer Community Plus Login User&apos;, 
{!$Profile.Name} = &apos;TPA Customer Community Plus Login User&apos;,
{!$Profile.Name} = &apos;Customer Community Plus Login User&apos;,
{!$Profile.Name} = &apos;Super User Profile CCP&apos;,
{!$Profile.Name} = &apos;TPA Processor Profile CCP&apos;,
{!$Profile.Name} = &apos;US TP Processor Profile CCP&apos;,
BEGINS({!$Record.Subject}, &apos;TST_CAS_FLW_CSM_Case_SendAllEmailAlerts:Tech:Portal&apos;)  ),
{!$Record.Account_Name_Article__c} &lt;&gt; &apos;LUPIN&apos;, 
(ISPICKVAL({!$Record.Origin}, &apos;Customer Portal&apos;)),
NOT({!$Record.Contact.Inactive__c}),
NOT({!$Record.Contact.EmailOptOut__c}),
OR (ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Yes&apos;), 
AND( 
{!$Record.Account.SendAutomaticCaseAcknowledgmentEmail__c}, 
ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Account choice&apos;))))</expression>
    </formulas>
    <formulas>
