    <formulas>
        <name>Tech_E2C</name>
        <dataType>Boolean</dataType>
        <expression>AND(
{!$Record.RecordTypeName__c} = &apos;TechnologyCase&apos;, 
{!$Record.CreatedById} = &apos;0056A000000jAPAQA2&apos;, /* Auto case System User */ 
{!AllLastModifiedMinusCreatedDate}, 
OR(
AND( 
NOT(ISBLANK({!$Record.AccountId})),
OR (ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Yes&apos;), 
AND( 
{!$Record.Account.SendAutomaticCaseAcknowledgmentEmail__c}, 
ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Account choice&apos;)))),
ISBLANK({!$Record.AccountId})
))</expression>
    </formulas>
    <formulas>
        <name>Tech_E2C_Apex</name>
        <dataType>Boolean</dataType>
        <expression>AND( 
{!$Record.RecordTypeName__c} = &apos;TechnologyCase&apos;, 
OR({!$Record.CreatedById} = &apos;0056A000000jAPAQA2&apos;,{!$Record.CreatedById} = &apos;0056A000000jAPA&apos;), /* Auto case System User */ 
ISCHANGED({!$Record.From_EmailToCase__c}), 
ISBLANK(PRIORVALUE({!$Record.From_EmailToCase__c})),
NOT(ISBLANK({!$Record.AccountId})),
NOT(ISBLANK({!$Record.ContactId})),
OR (
ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Yes&apos;), 
AND( 
{!$Record.Account.SendAutomaticCaseAcknowledgmentEmail__c} , 
ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Account choice&apos;)))
)</expression>
    </formulas>
    <formulas>
        <name>Tech_E2C_Apex_Resolved</name>
        <dataType>Boolean</dataType>
        <expression>AND(
NOT({!$Record.Low_Email_Notification_frequency__c}),
NOT(ISPICKVAL(PRIORVALUE({!$Record.Status}),&apos;Resolved with customer&apos;)),  
ISPICKVAL({!$Record.Status},&apos;Resolved with Customer&apos;), {!$Record.SendResolutionEmail__c}, 
{!$Record.RecordTypeName__c} = &apos;TechnologyCase&apos;, 
NOT(ISBLANK({!$Record.From_EmailToCase__c})), 
{!$Record.From_EmailToCase__c} &lt;&gt; &apos;ivpsupport@iqvia.com&apos;,
NOT(ISBLANK({!$Record.ContactId})),
NOT(ISBLANK({!$Record.AccountId})),	
{!$Record.Contact.EmailOptOut__c} = False,
OR(
ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Yes&apos;),  
AND( 
{!$Record.Account.SendAutomaticCaseAcknowledgmentEmail__c}, 
ISPICKVAL ({!$Record.SendAutomaticAcknowledgmentEmail__c},&apos;Account choice&apos;))))</expression>
    </formulas>
