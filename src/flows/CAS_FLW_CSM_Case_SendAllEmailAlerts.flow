        <dataType>Boolean</dataType>
        <expression>(({!$Record.LastModifiedDate} - {!$Record.CreatedDate})*86400 &lt; {!$Setup.CSM_case_Trigger_Handler__c.Difference_By_Last_Created_Time_Case__c})</expression>
    </formulas>
