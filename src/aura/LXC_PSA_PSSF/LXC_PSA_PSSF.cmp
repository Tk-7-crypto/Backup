    <aura:attribute name="isOpen" type="boolean" default="false"/>
    <aura:if isTrue="{!v.isOpen}">
        
        <!--###### MODAL BOX Start######--> 
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                <!-- ###### MODAL BOX HEADER Start ######-->
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{! c.closeModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Versions</h2>
                </header>
                <!--###### MODAL BOX BODY Part Start######-->
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    <force:recordData aura:id="recordHandler"
                                        recordId="{!v.versionPssfId}"
                                        fields="Id,Project_Name__c"
                                        targetFields="{!v.pssfObject}"              
                                        targetError="{!v.recordError}"
                                        recordUpdated="{!c.handleRecordUpdated}" />
                    <table class="slds-table slds-table--bordered slds-table--cell-buffer">
                        <thead>
                            <tr class="slds-text-title--caps">
                                <th scope="col">
                                    <div class="slds-truncate" title="PSSF Version">PSSF Version</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="Published By">Published By</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="Published on">Published on</div>
                                </th>
                                <aura:if isTrue="{!v.hasDeletePermission}">
                                    <th scope="col">
                                        <div class="slds-truncate" title="Delete">Delete</div>
                                    </th>
                                </aura:if>
                            </tr>
                        </thead>
                        <tbody>
                            <aura:iteration items="{!v.PSSFList}" var="ct">
                                <tr>
                                    <td data-label="PSSF Version">
                                        <div class="slds-truncate textColor" title="{! ct.PSSF_Version__c }" onclick="{! c.openVersion }" data-target-id="{! ct.Id }">{! ct.PSSF_Version__c }</div>
                                    </td>
                                    <td data-label="Published By">
                                        <div  title="{!ct.CreatedBy.Name}">{! ct.CreatedBy.Name }</div>
                                    </td>
                                    <td data-label="Published on">
                                        <lightning:recordViewForm recordId="{!ct.Id}" objectApiName="PSSF__c">
                                            <lightning:outputField fieldName="CreatedDate" variant="label-hidden" />
                                        </lightning:recordViewForm>
                                    </td>
                                    <aura:if isTrue="{!v.hasDeletePermission}">
                                        <td data-label="Delete Version">
                                                <lightning:button label="Delete" variant="neutral" onclick="{!c.setDeletingVersionId}" value="{!ct.Id}"/>
                                        </td>
                                    </aura:if>
                                </tr>
                            </aura:iteration>
                        </tbody>
                    </table>
                </div>
                <!--###### MODAL BOX FOOTER Part Start ######-->
                <footer class="slds-modal__footer">
                    <lightning:button variant="neutral" 
                                      label="Cancel"
                                      title="Cancel"
                                      onclick="{! c.closeModel }"/>
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
        <!--###### MODAL BOX Part END Here ######-->
        
    </aura:if>
    
    <!-- Dialog box to confirm deletion of PSSF Version-->
    <aura:attribute name="isDeletionConfirmationOpen" type="boolean" default="false"/>
    <aura:if isTrue="{!v.isDeletionConfirmationOpen}">
        <!--###### MODAL BOX Start######--> 
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                <!-- ###### MODAL BOX HEADER Start ######-->
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{! c.closeModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
                </header>
                <!--###### MODAL BOX BODY Part Start######-->
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    Are you sure you want to delete this PSSF?
                </div>
                <!--###### MODAL BOX FOOTER Part Start ######-->
                <footer class="slds-modal__footer">
                    <lightning:button variant="neutral" 
                                      label="Delete"
                                      title="Delete"
                                      onclick="{! c.handleDeleteRecord }"/>
                    <lightning:button variant="neutral" 
                                      label="Cancel"
                                      title="Cancel"
                                      onclick="{! c.closeModel }"/>
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
        <!--###### MODAL BOX Part END Here ######-->
        
    </aura:if>
    
    <aura:attribute name="isOpenPDF" type="boolean" default="false"/>
    <aura:if isTrue="{!v.isOpenPDF}">
        <!--###### MODAL BOX Start######--> 
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container pdfmodaldiv">
                <!-- ###### MODAL BOX HEADER Start ######-->
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{! c.closePDFModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">PSSF</h2>
                </header>
                <!--###### MODAL BOX BODY Part Start######-->
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    <iframe src="{!v.vfpageurl}" width="100%" height="500px;" frameBorder="0"/>
                </div>
                <!--###### MODAL BOX FOOTER Part Start ######-->
                <footer class="slds-modal__footer">
                    <lightning:button variant="neutral" 
                                      label="Cancel"
                                      title="Cancel"
                                      onclick="{! c.closePDFModel }"/>
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
        <!--###### MODAL BOX Part END Here ######-->
    </aura:if>
</aura:component>
