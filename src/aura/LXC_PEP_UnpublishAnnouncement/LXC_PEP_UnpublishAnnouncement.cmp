<aura:component controller="CNT_PEP_ManageAnnouncement"
                implements="force:hasRecordId,forceCommunity:availableForAllPageTypes,flexipage:availableForAllPageTypes,force:lightningQuickActionWithoutHeader,force:appHostable"
                access="global">
    <aura:handler name="init" action="{!c.unPublishAnnouncement}" value="{!this}"/>
    <aura:attribute name="status" type="String" default="Unpublishing announcement"/>
    
    <div class="slds-align_absolute-center" style="height: 5rem;">
    	<p>{!v.status}</p>
    </div>
        
</aura:component>