<aura:event type="APPLICATION" description="Event template" >
    <aura:attribute name="title" type="String" description="This is the title of the model" />
    <aura:attribute name="selectedList" type="List" description="Product that has been selected by user" />
    <aura:attribute name="isFadeIn" type="Boolean" description="fade in flag for model" />
    <aura:attribute name="operationType" type="String" description="type of operation" />
    <aura:attribute name="fieldsToDisable" type="String" description="Give Api Name of fields which have to disabled in operationType edit"/>
    <aura:attribute name="fieldsToShow" type="String" description="Give Api Name of fields which have to show in operationType create and edit"/>
    <aura:attribute name="territory" type="String"/>
</aura:event>