            <property label="File Data" name="fData" type="String"/>
            <property label="Label" name="label" role="inputOnly" type="String"/>
            <property label="Mandatory" name="isMandatory" type="Boolean"/>
            <property label="Del - Title" name="titles" role="outputOnly" type="String"/>
            <property label="Title" name="title" type="String"/>
            <property label="File Wrapper List" name="wrapperList" type="apex://FileWrapper[]"/>
