                document.getElementById(decodeURIComponent(txtId) + '_lkid').value=data[0];
                document.getElementById(decodeURIComponent(txtId) + '_lkold').value=data[1];
                document.getElementById(decodeURIComponent(txtId) + '_mod').value=1;
