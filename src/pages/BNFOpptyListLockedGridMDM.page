                document.getElementById(decodeURIComponent(txtId) + '_lkold').value='';
                document.getElementById(decodeURIComponent(txtId) + '_mod').value=0;
                document.getElementById(decodeURIComponent(txtId)).value='';
            }
            
            if(data[2]!=null && data[2]!='')
