                }
                .additionalContacts {
                    table-layout: auto !important;
                }
            </style>
            <script src="{!$Resource.CDAJQuery}" type="text/javascript"></script>   <!-- *** Jquery library reference *** -->
            <script src="{!URLFOR($Resource.CDACustomPopup, 'popup.js')}" type="text/javascript"></script>  <!-- *** JQuery Custom Popup Library reference *** -->
            <link href="{!URLFOR($Resource.CDACustomPopup, 'popup.css')}" rel="stylesheet"/>    <!-- *** JQuery Custom Popup Stylesheet reference *** -->
            <script>
                // Added by Babita Dadarwal under CR-11659 Start
                $( document ).ready(function() {
                    if('{!$CurrentPage.parameters.status}' == 'Sent for Recipient Signature') {
                        popupAlert('{!$Label.CDA_The_CDA_is_sent_for_Signature}', function () {
                            // window.location = '/apex/VFP_CDA_RequestViewScreen?id={!$CurrentPage.parameters.id}';
                            navigateToRequestViewScreen();
                        });
                    }
                    //
                    console.log('isLookupReportUser: '+{!isLookupReportUser}+', isCurrentUserARequestOwner: '+{!isCurrentUserARequestOwner}+', isNegotiator: '+{!isNegotiator});
                    if('{!isLookupReportUser}' == 'true' || ('{!isNegotiator}' == 'false' && '{!isCurrentUserARequestOwner}' == 'false')) {
                        $("#overlay").show();
                    }
                });
                // Added by Babita Dadarwal under CR-11659 End

                function setFocusOnLoad() {}    //Added by Vikram Singh under Item #190

                //Navigation to Request Edit Page
                navigateToRequestEditScreen = function() {
                    var recordId = "{!$CurrentPage.parameters.id}";
                    var componentAttributes = {"recordId" : recordId};
                    var componentDef = "c:LXC_CDA_RequestEditScreen";
                    if(SfdcApp && SfdcApp.projectOneNavigator) {
                        SfdcApp.projectOneNavigator.fireContainerEvent("force:navigateToComponent", {componentDef : componentDef, componentAttributes : componentAttributes});
                    }
                }

                //Navigation to Request View Page
                navigateToRequestViewScreen = function() {
                    sforce.one.navigateToURL('/apex/VFP_CDA_RequestViewScreen?id={!$CurrentPage.parameters.id}', true);
                }

                //Navigation to Custom Landing Page
                navigateToLandingScreen = function() {
                    var componentDef = "c:LXC_CDA_LandingPage";
                    if(SfdcApp && SfdcApp.projectOneNavigator) {
                        SfdcApp.projectOneNavigator.fireContainerEvent("force:navigateToComponent", {componentDef : componentDef});
                    }
                }

                popupAlert = function (msg, truePart) {
                    popup(msg, {
                        'animate': true
                    }, function (r) {
                        if (r && truePart != null)
                            truePart();
                        }
                    );
                    $.each($(".aButtons button"), function(k, v) {
                        $(v).attr( "class", "slds-button slds-button_outline-brand" );
                    });
                    window.scrollTo(0, 0);
                }

                popupConfirm = function (msg, truePart, falsePart, isYesNo) {
                    if (isYesNo)
                        popup(msg, {
                            'verify': true,
                            'animate': true
                        }, function (r) {
                            if (r)
                                truePart();
                            else
                                falsePart();
                            }
                        );
                    else
                        popup(msg, {
                            'confirm': true,
                            'animate': true
                        }, function (r) {
                            if (r)
                                truePart();
                            else
                                falsePart();
                        }
                    );
                    $.each($(".aButtons button"), function(k, v) {
                        $(v).attr( "class", "slds-button slds-button_outline-brand" );
                    });
                    window.scrollTo(0, 0);
                }

                // Added by Ajinkya Pande under Item #162 Start
                var isTextAreaFocus = false;
                popupPrompt = function (msg, truePart, falsePart) {
                    isTextAreaFocus = true;
                    popup(msg, {
                        'input' : true,
                        'animate': true
                    }, function (r) {
                        if(r) {
                            truePart(r);
                        } else {
                            falsePart();
                        }
                        isTextAreaFocus = false;
                    });
                    $.each($(".aButtons button"), function(k, v) {
                        $(v).attr( "class", "slds-button slds-button_outline-brand" );
                    });
                    window.scrollTo(0, 0);
                }
                // Added by Ajinkya Pande under Item #162 End

                function uploadDocument(attachmentType) {
                    // window.location = '/apex/VFP_CDA_CustomDocumentUploadPage?id=' + '{!CDA_Request__c.id}' + '&attachmentType=' + attachmentType;
                    sforce.one.navigateToURL('/apex/VFP_CDA_CustomDocumentUploadPage?id={!$CurrentPage.parameters.id}' + '&attachmentType=' + attachmentType, true);
                }

                function submitPopup() {
                    var successMessage = '{!$Label.CDA_The_CDA_request_has_been_received_and_the_Recipient_s_point_of_contact_will}';
                    var successMessageForExternal = '{!$Label.The_CDA_request_was_submitted_and_sent_to_a_CDA_negotiator_for_review}';
                    var successMessageForPdfSign = '{!$Label.The_CDA_has_been_submitted_and_sent_to_the_recipient_for_signature}';
                    var successMessageForOGC = '{!$Label.CDA_The_CDA_request_has_been_sent_to_the_CDA_Negotiator_to_review_and_obtain_app}'; //Added by Ajinkya Pande under Item #35
                    var successMessageForProcessBatch = '{!$Label.CDA_Your_CDA_request_has_been_submitted_for_processing_You_will_receive_an_emai}';  //Added by Vikram Singh under CR-11525

                    if((!{!isUserHavingApttusLicense} || ({!isUserHavingApttusLicense} && !{!isNegotiator})) && '{!CDA_Request__c.CDA_Source__c}' != 'External') {  //Added by Vikram Singh under CR-11525
                        successMessage = successMessageForProcessBatch;    //Added by Vikram Singh under CR-11525
                    }else if('{!CDA_Request__c.Competitor_System_Access__c}' == 'Yes') { //Added by Ajinkya Pande under Item #35    //Updated by Vikram Singh under CR-11525
                        successMessage = successMessageForOGC; //Added by Ajinkya Pande under Item #35
                    } else if('{!CDA_Request__c.CDA_Source__c}' == 'External') { //Updated by Ajinkya Pande under Item #35
                        successMessage = successMessageForExternal;
                    } else if('{!CDA_Request__c.CDA_Source__c}' == 'IQVIA' && '{!CDA_Request__c.CDA_Format__c}' == 'PDF' && '{!CDA_Request__c.Competitor_System_Access__c}' != 'Yes') {  //Modified by Vikram Singh under CR-11577
                        successMessage = successMessageForPdfSign;
                    }

                    popupAlert(successMessage, function() {
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    });
                }

                //Added by Vikram Singh under CR-11525 Start
                function confirmSubmissionPopup() {
                    popupConfirm('{!$Label.CDA_Are_you_sure_you_want_to_submit_the_request}', function() {
                        showLoading();
                        submitRequest();
                    }, function() {});
                    $.each($(".aButtons button"), function(k, v){
                        if($(v).text() == 'Ok')
                            $(v).html('Yes');
                    });
                }
                //Added by Vikram Singh under CR-11525 End

                function saveForLaterPopup() {
                    popupAlert('{!$Label.CDA_The_CDA_request_has_been_saved_and_is_available_for_further_editing}', function() {
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    });
                }

                function sendForReviewPopup()  {
                    popupAlert('{!$Label.CDA_CDA_was_send_to_the_recipient_for_review}', function() {
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    });
                }

                function confirmForLanguageReviewPopup() {
                    if ('{!CDA_Request__c.CDA_Language__c}' == 'English') {
                        popupAlert('{!$Label.CDA_The_MyCDA_Legal_POC_assignment_process_is_only_for_non_English_languages}', function () { });
                    } else {
                        // showLoading();
                        sendForLanguageReview();
                    }
                }

                function sendForLanguageReviewPopup() {                    
                    popupAlert('{!$Label.CDA_Please_confirm_the_documentation_requires_non_English_review_processing_the}', function () {
                        navigateToLandingScreen();
                    });
                }

                function voidEnvelopeAndResubmitPopup() {
                    popupConfirm('{!$Label.CDA_Are_you_sure_you_want_to_recall_and_resubmit_the_request}', function() {
                        showLoading();
                        voidEnvpandResubmitRequest();
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    }, function() {});
                    $.each($(".aButtons button"), function(k, v){
                        if($(v).text() == 'Ok')
                            $(v).html('Acknowledge');
                    });
                }

                function voidEnvelopeAndCancelPopup() {
                    popupConfirm('{!$Label.CDA_Are_you_sure_you_want_to_recall_and_cancel_the_request}', function() {
                        showLoading();
                        voidAndCancelRequest();
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    }, function() {});
                    $.each($(".aButtons button"), function(k, v){
                        if($(v).text() == 'Ok')
                            $(v).html('Acknowledge');
                    });
                }

                function updateStatusPopup() {
                    var cdaNewStatus = $("[id$='cdaStatusUpdateId']").val();
                    if({!!isCompletedDocAttached} && cdaNewStatus == 'Contract Executed') {
                        popupAlert('You can\'t update status until "Completed Document" is attached. The format of document should be CDA-XXXXXX_Completed with extension pdf/doc/docx. e.g. CDA-<CDA Number>_Completed.pdf');
                    } else {
                        showLoading();
                        updateCdaStatus();
                    }
                }

                //Added by Babita Dadarwal under CR-11458 start
                function showPartiallyExecutedCdaPopup() {
                    popupConfirm('{!$Label.CDA_IMPORTANT_Only_use_this_option_when_a_CDA_is_an_IQVIA_template_is_already}', function() {
                        uploadDocument('Partially Executed CDA Document');
                    }, function() {});
                }
                //Added by Babita Dadarwal under CR-11458 End

                function cancelbyRequestorPopup() {
                    popupConfirm('{!$Label.CDA_Are_you_sure_you_want_to_cancel_the_request}', function(){
                        showLoading();
                        cancelbyRequestor();
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    }, function() {});
                    $.each($(".aButtons button"), function(k, v) {
                        if($(v).text() == 'Ok')
                            $(v).html('Acknowledge');
                    });
                }

                // Added by Ajinkya Pande under Item #162 Start
                function respondToRequestorPopup() {
                    popupPrompt('{!$Label.CDA_Reply_Response}', function(msg) {
                        showLoading();
                        respondToRequestorEmail(msg);
                    }, function() {});
                }

                function successRespondToRequestPopup() {
                    popupAlert('{!$Label.CDA_Reply_Response_Sent}', function() {
                        showLoading();
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    });
                }
                // Added by Ajinkya Pande under Item #162 End

                // Added by Ajinkya Pande under Item #1002 Start
                function sendNotificationEmailPopup() {
                    popupAlert('{!$Label.CDA_Executed_agreement_notification_emails_have_been_sent}', function() {
                        // window.location = '/apex/VFP_CDA_CustomLandingPage';
                        navigateToLandingScreen();
                    });
                    $.each($(".aButtons button"), function(k, v) {
                        if($(v).text() == 'Ok')
                            $(v).html('Acknowledge');
                    });
                }
                // Added by Ajinkya Pande under Item #1002 End

                var manualValidation = function(validationType) {
                    var isShowError = false;
                    var countErrors = 0;
                    $('[id$=errMsg]').hide();
                    $('[id$=errMsg]').html('<div class="message errorM3" role="alert"><table border="0" cellpadding="1" cellspacing="1" class="messageTable" style="padding:0px;margin:0px;"><tr valign="top"><td><img alt="ERROR" class="msgIcon" src="/s.gif" title="ERROR"/></td><td class="messageCell"><div class="messagetext"><span style="color:#cc0000"><h4>Errors</h4></span></div></td></tr><tr><td></td><td><span><ul></ul></span></td></tr></table></div>')

                    if(validationType == 'AuthSignerUpdate' && $("[id$='recipientAuthorizedSignerOptions']").val() == 'No') {
                        var recipientAuthorizedSignerFirstNameVal = $("[id$='recipientAuthorizedSignerFirstName']").val();
                        var recipientAuthorizedSignerLastNameVal = $("[id$='recipientAuthorizedSignerLastName']").val();
                        var recipientAuthorizedSignerTitleVal = $("[id$='recipientAuthorizedSignerTitle']").val();
                        var recipientAuthorizedSignerEmailAddressVal = $("[id$='recipientAuthorizedSignerEmailAddress']").val();
                        var recipientAuthorizedSignerConfirmEmailAddressVal = $("[id$='recipientAuthorizedSignerConfirmEmailAddress']").val();

                        if(recipientAuthorizedSignerFirstNameVal != undefined && (recipientAuthorizedSignerFirstNameVal == '' || recipientAuthorizedSignerFirstNameVal == null)) {
                            $("[id$='RecipientAuthorizedSignerFirstNameError']").html('<strong>Error:</strong> You must enter a value');
                            isShowError = true;
                            countErrors++;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">Recipient Authorized Signer First Name: You must enter a value</li>');
                            }
                        }
                        if(recipientAuthorizedSignerLastNameVal != undefined && (recipientAuthorizedSignerLastNameVal == '' || recipientAuthorizedSignerLastNameVal == null)) {
                            $("[id$='RecipientAuthorizedSignerLastNameError']").html('<strong>Error:</strong> You must enter a value');
                            isShowError = true;
                            countErrors++;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">Recipient Authorized Signer Last Name: You must enter a value</li>');
                            }
                        }
                        if(recipientAuthorizedSignerTitleVal != undefined && (recipientAuthorizedSignerTitleVal == '' || recipientAuthorizedSignerTitleVal == null)) {
                            $("[id$='RecipientAuthorizedSignerTitleError']").html('<strong>Error:</strong> You must enter a value');
                            isShowError = true;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">Recipient Authorized Signer Title: You must enter a value</li>');
                            }
                        }
                        if(recipientAuthorizedSignerEmailAddressVal != undefined && (recipientAuthorizedSignerEmailAddressVal == '' || recipientAuthorizedSignerEmailAddressVal == null)) {
                            $("[id$='recipientAuthorizedSignerEmailAddressError']").html('<strong>Error:</strong> You must enter a value');
                            isShowError = true;
                            countErrors++;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">Recipient Authorized Signer Email Address: You must enter a value</li>');
                            }
                        } else if(recipientAuthorizedSignerEmailAddressVal != undefined && (recipientAuthorizedSignerEmailAddressVal != '' || recipientAuthorizedSignerEmailAddressVal != null) && !isValidEmail(recipientAuthorizedSignerEmailAddressVal)){
                            $("[id$='recipientAuthorizedSignerEmailAddressError']").html('<strong>Error:</strong> Invalid Email Address');
                            isShowError = true;
                            countErrors++;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">Recipient Authorized Signer Email Address: Invalid Email Address</li>');
                            }
                        }
                        if(recipientAuthorizedSignerConfirmEmailAddressVal != undefined && (recipientAuthorizedSignerConfirmEmailAddressVal == '' || recipientAuthorizedSignerConfirmEmailAddressVal == null)) {
                            $("[id$='recipientAuthorizedSignerConfirmEmailAddressError']").html('<strong>Error:</strong> You must enter a value');
                            isShowError = true;
                            countErrors++;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">Recipient Authorized Signer Confirm Email Address: You must enter a value</li>');
                            }
                        } else if (recipientAuthorizedSignerConfirmEmailAddressVal != undefined && recipientAuthorizedSignerEmailAddressVal != undefined && (recipientAuthorizedSignerConfirmEmailAddressVal.toLowerCase() != recipientAuthorizedSignerEmailAddressVal.toLowerCase())) {
                            $("[id$='recipientAuthorizedSignerConfirmEmailAddressError']").html('<strong>Error:</strong> Email does not match the confirm Email');
                            isShowError = true;
                            countErrors++;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">Recipient Authorized Signer Confirm Email Address: Email does not match the confirm Email.</li>');
                            }
                        }
                    }

                    //Added by Vikram Singh under CR-11146 Start
                    if(validationType == 'ResponseDocType') {
                        var cdaRequestResponseDocTypeVal = $("[id$='cdaRequestResponseDocType']").val();

                        if(cdaRequestResponseDocTypeVal != undefined && (cdaRequestResponseDocTypeVal == '' || cdaRequestResponseDocTypeVal == null)) {
                            $("[id$='cdaRequestResponseDocTypeError']").html('<strong>Error:</strong> You must enter a value');
                            isShowError = true;
                            countErrors++;
                            if($('[id$=errMsg] ul').html() != null) {
                                $('[id$=errMsg] ul').html($('[id$=errMsg] ul').html()+'<li style="padding-top:5px">What documents would you like to provide: You must enter a value</li>');
                            }
                        }
                    }
                    //Added by Vikram Singh under CR-11146 End

                    if(isShowError) {
                        if(countErrors == 1)
                            $('[id$=errMsg] h4').html('Error');
                        $('[id$=errMsg]').show();
                        hideLoading();
                    }
                    if(!isShowError){
                        if(validationType == 'AuthSignerUpdate') {
                            updateAuthSignDetails();
                        }
                        //Added by Vikram Singh under CR-11146 Start
                        if(validationType == 'ResponseDocType') {
                            setAndUpdateResponseDocumentType();
                        }
                        //Added by Vikram Singh under CR-11146 End
                    }
                }

                function showCharCount(myTA, maxSize, sizePanelId) {
                    $('[id$="'+ sizePanelId +'"]').html('*' + (maxSize - myTA.value.length) + ' characters remaining');
                }
            </script>
            <apex:slds />
            <apex:includeLightning />
        </head>
        <body style="background-image: url(/_slds/images/themes/lightning_blue/lightning_blue_background.png),linear-gradient(to top, rgba(160, 180, 206, 0.0) 0, rgba(160, 180, 206, 1.0)); background-repeat: no-repeat; background-repeat: top left;">
            <div id="overlay" class="readonly"></div>
            <div class="slds-scope">
                <c:VFC_CDA_LoadingImage />
                <div id="systemDownMessageCmp" />
                <script>
                	showLoading();
                	$Lightning.use("c:LXA_CDA_SystemDownTime", function() {
                        $Lightning.createComponent("c:LXC_CDA_DownTimeMessage", {isShowHeader: true, isMessageDownType: true}, "systemDownMessageCmp", function(cmp) {hideLoading();});
                    });
                </script>
                <!-- PAGE HEADER -->
    			<div class="slds-page-header" role="banner" style="margin: -10px 12px 12px; border-radius: 4px;">
    				<div class="slds-grid">
    					<div class="slds-col slds-has-flexi-truncate">
                            <div class="slds-media slds-no-space slds-grow">
                                <span class="slds-icon_container slds-icon-custom-custom62 slds-media__figure" title="CDA Request">
                                    <svg aria-hidden="true" class="slds-icon">
                                        <use xlink:href="{!URLFOR($Asset.SLDS, '/assets/icons/custom-sprite/svg/symbols.svg#custom62')}"></use>
                                    </svg>
                                    <span class="slds-assistive-text">CDA Request Icon</span>
                                </span>
        						<!-- HEADING AREA -->
                                <div class="slds-media__body">
                                    <p class="slds-breadcrumb__item slds-line-height_reset">CDA Request</p>
                                    <h1 class="slds-page-header__title slds-truncate" title="{!If(CDA_Request__c.CDA_Id__c=='','New CDA Request',CDA_Request__c.Name) }">{!If(CDA_Request__c.CDA_Id__c=='','New CDA Request',CDA_Request__c.Name) }</h1>
    							</div>
                                <!-- / HEADING AREA -->
        					</div>
                        </div>
    				</div>
    			</div>
    			<!-- / PAGE HEADER -->
                <apex:form id="editForm" style="margin: 0px 12px;">
                    <apex:Pagemessages id="errMsg" />
                    <apex:actionFunction name="updateAuthSignDetails" action="{!updateAuthSignerDetails}" reRender="errMsg"/>
                    <apex:actionFunction name="isTheRecipientAuthorizedToSign" oncomplete="hideLoading();" reRender="errMsg, authSignerDetails" >   <!-- Updated by Vikram Singh under CR-11146 -->
                        <apex:param name="isAuthorizedSigner" value="" assignTo="{!isAuthorizedSigner}" />
                    </apex:actionFunction>
                    <apex:actionFunction name="voidEnvpandResubmitRequest" action="{!voidDocusignEnvpAndResubmit}" reRender="errMsg" oncomplete="hideLoading();"/>
                    <apex:actionFunction name="voidAndCancelRequest" action="{!voidDocusignEnvpAndCancel}" reRender="errMsg" oncomplete="hideLoading();"/>
                    <apex:actionFunction name="updateCdaStatus" action="{!updateCdaStatus}"  reRender="errMsg, basicInfoBlockSection" oncomplete="hideLoading();"/>  <!-- Updated by Vikram Singh under Issue-11192 -->
                    <apex:actionFunction name="cancelbyRequestor" action="{!cancelbyRequestor}" reRender="errMsg" oncomplete="hideLoading();"/>
                    <!-- Added by Ajinkya Pande under Item #162 Start -->
                    <apex:actionFunction name="respondToRequestorEmail" action="{!respondToRequestorEmail}" reRender="errMsg" oncomplete="successRespondToRequestPopup();hideLoading();">
                      <apex:param name="replyResponse" assignTo="{!cdaRequest.Requestor_Reply_Response__c}" value="" />
                    </apex:actionFunction>
                    <!-- Added by Ajinkya Pande under Item #162 End -->
                    <apex:actionFunction name="setAndUpdateResponseDocumentType" action="{!setAndUpdateResponseDocumentType}" oncomplete="hideLoading();" reRender="errMsg, pbsDocType, actionSection" /> <!-- Added By Vikram Singh under CR-11146 -->
                    <apex:actionFunction name="submitRequest" action="{!submitRequest}" reRender="errMsg" oncomplete="submitPopup();hideLoading();"/> <!-- Added by Vikram Singh under CR-11525 -->
                    <apex:actionFunction name="sendForLanguageReview" action="{!sendForLanguageReview}" reRender="errMsg" oncomplete="sendForLanguageReviewPopup();hideLoading();" />

                    <apex:pageBlock title="{!$Label.CDA_Information_Label}" id="InformationBlock">
                        <!-- Added By Vikram Singh under CR-11146 Start -->
                        <apex:pageBlockSection id="pbsDocType">
                            <apex:pageBlockSectionItem id="docTypeViewMode" rendered="{!!isCdaRequestResponseDocTypeEditable && isRedlineUploadHasToRender && (isRedLineDocumentRequired || isRegenrateDocByNegotiator)}">
                                <apex:outputPanel layout="block" styleClass="slds-form-element">
                            		<c:VFC_CDA_LightningHelpText cLblTxt="{!$ObjectType.CDA_Request__c.fields.What_documents_would_you_like_to_provide__c.Label}" cHlpTxt="{!$ObjectType.CDA_Request__c.fields.What_documents_would_you_like_to_provide__c.InlineHelpText}"/>
                            		<div class="slds-form-element__control slds-has-divider--bottom">
