<apex:page id="editPage" showHeader="false" standardController="CDA_Request__c" standardStylesheets="false" lightningStyleSheets="true" extensions="EXT_CDA_RequestViewController" action="{!redirectHistoricalDataLayout}" language="en_US" docType="html-5.0">    <!-- Updated by Vikram Singh under CR-11691 -->
    <apex:includeLightning />
    <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" lang="en">
        <head>
            <style type="text/css">
                .helpText {
                    width: 27em !important;
                }
                .negotiatorDetails .requiredBlock {
                    display: none !important;
                }
                .myBtnDisabled {
                    /*color : rgba(255, 0, 0, 0.62) !important;*/
                    color: red !important;
                }
                .opQiLegalEntity a{
                    pointer-events: none;
                    text-decoration: none;
                }
                .slds-scope .slds-media__body{
                    color: rgb(62, 62, 60) !important;
                }
                .slds-scope .slds-form-element__control{
                    color: rgb(43, 40, 38) !important;
                }
                .readonly{
                    position: fixed;
                    display: none !important;
                    width: 100%;
                    height: 100%;
                    top: 0;
                    left: 0;
                    right: 0;
                    bottom: 0;
