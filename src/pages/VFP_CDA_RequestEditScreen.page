        function validateDateRange() {
            var selectedDate = $("[id$='effectiveDate']").val();
            if(selectedDate == null) {
                return false;
            }
            selectedDate = new Date(selectedDate);
            var todayDate = new Date();
            todayDate = new Date((todayDate.getMonth()+1) + "/" + todayDate.getDate() + "/" + todayDate.getFullYear());
            var millisecondsPerDay = 1000 * 60 * 60 * 24;
            var millisBetween = selectedDate.getTime() - todayDate.getTime();
            var days = Math.floor(millisBetween / millisecondsPerDay);
            // Modified by C.P.Pandey under CR-11578 start
            if((days < -60 || days > 90) && !isNegotiator) {
                alert("{! $Label.You_cannot_select_date_60_days_prior_or_90_days_after_the_current_date}");
                $("[id$='effectiveDate']").val(null);
            }
            else if((days < -365 || days > 90) && isNegotiator) {
                alert("{! $Label.You_cannot_select_date_1_year_prior_or_90_days_after_the_current_date}");
                $("[id$='effectiveDate']").val(null);
            }
            // Modified by C.P.Pandey under CR-11578 end
        }

