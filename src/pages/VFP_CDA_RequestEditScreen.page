                        popupConfirm('{!$Label.CDA_Confidentiality_Popup}',
                        function() {
                            cdaRequestDelete();
                        }, function() {
                            $("[id$='empClientCustomerId']").val('--None--');
                        });
                        $.each($(".aButtons button"), function(k, v) {
                            if($(v).text() == 'Ok')
                                $(v).html('Acknowledge');
                        });
                    }
                }

                isCompetitorAuditor = function () {
                    var CompetitorAuditorVal = $("[id$='cdaType']").val();
                    if (CompetitorAuditorVal == 'Auditor') {
                        popupConfirm('{!$Label.CDA_Auditor_Identified_As_Compitator_Popup}', function(){
                            // cdaRequestDelete();
                            showLoading();
                            cdaRequestSave();
                        } , function() {
                            $("[id$='competitorFlagId']").val('--None--');
                            showLoading();
                            setSpecificIndicatorMethod_AF();
                        });
                        $.each($(".aButtons button"), function(k, v){
                            if($(v).text() == 'Ok')
                                $(v).html('Acknowledge');
                        });
                    }
                }

                setSpecificIndicator = function () {
                    var CompetitorFlagVal = $("[id$='competitorFlagId']").val();
                    var cdaType = $("[id$='cdaType']").val();
                    if (cdaType == 'Auditor') {
                        $("[id$='specificIndicatorId']").attr('disabled', 'disabled');
                    }
