                popupAlert = function (msg, truePart) {
                    popup(msg, {
                        'animate': true
                    }, function (r) {
                        if (r && truePart != null)
                            truePart();
                        }
                    );
                }

                popupConfirm = function (msg, truePart, falsePart, isYesNo) {
                    if (isYesNo)
                        popup(msg, {
                            'verify': true,
                            'animate': true
                        }, function (r) {
                            if (r)
                                truePart();
                            else
                                falsePart();
                            }
                        );
                    else
                        popup(msg, {
                            'confirm': true,
                            'animate': true
                        }, function (r) {
                            if (r)
                                truePart();
                            else
                                falsePart();
                            }
                        );
                    }

