                    popupConfirm(msg, function(){
                        cdaRequestDelete();
                    }, function(){
                        if(legacyImsOrFinance == 'finance') {
                            quintilesFinancePopup();
                        }else if(legacyImsOrFinance == 'ims') {
                            quintilesLegacyIMSPopup();
                        }else{
