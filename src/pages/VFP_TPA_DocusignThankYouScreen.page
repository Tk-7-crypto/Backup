<apex:page id="pg" standardController="Account" extensions="EXT_TPA_DocusignThankYouScreen" title="Thank You" sidebar="false" showHeader="false" standardStylesheets="false" language="{!vendorLanguageCode}" lightningStylesheets="true">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <title>Thank You</title>
        <meta name="description" content="" />
        <meta name="viewport" content="width=980" />

        <apex:stylesheet value="{!URLFOR($Resource.IMSBranding, '/web/css/styles.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.IMSBranding, '/web/css/vendor/tooltipster/tooltipster.css')}" />
        
        <style>
        	body{
        		background: url({!URLFOR($Resource.IMSBranding, '/web/img/bg-main.png')}) repeat-x !important;
        	}
        </style>
    </head>

    <body>
        <div id="header" >
            <apex:image width="" style="width: 172px;height:49px;" value="{!$Resource.IMS_Logo}" alt="{!$Label.TPA_IQVIA_Keyword} logo"/>
        </div>

        <div id="content" style="height:60%">
            <apex:form id="frm">
                <apex:outputPanel >
                   <div class="title" style="background-color:#0FA5EF!important"></div>
                   <div class="form-section">
                       <h2 style="text-align: center;">{!$Label.TPA_Docusign_Thank_You_Message}</h2>
                    </div>
                    <br />
                    <br />
                    <br />
                    <br />
                    <br />
                </apex:outputPanel>
            </apex:form>
        </div>
        
        <div id="footer">
            <div class="copyright_wrapper">
                <div class="copyright">
                    <ul class="item social_links">
                        <a href="https://twitter.com/intent/user?screen_name=IMSHealth" class="twitter">Twitter</a>
                        <a href="http://www.linkedin.com/company/ims-health" class="linkedin">LinkedIn</a>
                        <a href="https://www.facebook.com/imshealth" class="facebook">Facebook</a>
                    </ul>
                    <div class="copy" style="float: right">
                        Copyright &copy;2014 {!$Label.TPA_IQVIA_Keyword} Incorporated. All rights reserved.
                    </div>
                </div>
            </div>
            <div class="bottom-bg"></div>
        </div>
    </body>
</apex:page>