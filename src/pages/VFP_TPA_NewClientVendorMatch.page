                                                         <td class="slds-cell-wrap"><a target="_blank" ng-href="/{{account.id}}">{{account.id}}</a> </td>
                                                         <td align="center"> <input id="check3" type="checkbox" ng-checked="account.isAMAAccount"  ng-disabled="true" /></td>
